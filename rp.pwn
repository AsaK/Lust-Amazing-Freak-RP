CMD:aviso(playerid, params[])
{
	static
	    userid,
		reason[32];

    if (PlayerData[playerid][pAdmin] < 1)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "us[32]", userid, reason))
	    return SendSyntaxMessage(playerid, "/aviso [playerid/nome] [motivo]");

    if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "O ID é inválido.");

	if (PlayerData[userid][pAdmin] > PlayerData[playerid][pAdmin])
	    return SendErrorMessage(playerid, "O jogador tem um level de admin superior ao seu.");

	switch (++ PlayerData[userid][pWarnings])
	{
	    case 1:
	    {
	        format(PlayerData[userid][pWarn1], 32, reason);

	        SendAdminAction(userid, "%s enviou um aviso para você. Motivo: \"%s\" (Primeiro aviso).", ReturnName(playerid, 0), reason);
	        SendAdminAction(playerid, "Você enviou um aviso para %s. Motivo \"%s\" (Primeiro aviso).", ReturnName(userid, 0), reason);
		}
		case 2:
	    {
	        format(PlayerData[userid][pWarn2], 32, reason);

	        SendAdminAction(userid, "%s enviou um aviso para você \"%s\" (Segundo aviso).", ReturnName(playerid, 0), reason);
	        SendAdminAction(playerid, "Você enviou um aviso para %s. Motivo \"%s\" (Segundo aviso).", ReturnName(userid, 0), reason);
		}
		default:
	    {
	        ResetWarnings(userid);

	        SendAdminAction(userid, "Você foi banido por ganhar 3 avisos (\"%s\").", reason);
	        SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s foi banido por ter 3 avisos por %s, motivo: %s", ReturnName(userid, 0), ReturnName(playerid, 0), reason);

			Blacklist_Add(PlayerData[userid][pIP], PlayerData[userid][pUsername], PlayerData[playerid][pUsername], reason);
