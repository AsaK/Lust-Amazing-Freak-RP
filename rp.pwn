CMD:pm(playerid, params[])
{
	static
	    userid,
	    text[128];

	if (sscanf(params, "us[128]", userid, text))
	    return SendSyntaxMessage(playerid, "/pm [playerid/nome] [mensagem]");

	if (PlayerData[playerid][pDisablePM])
		return SendErrorMessage(playerid, "Você precisa ativar as mensagens privadas.");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "O ID é inválido.");

	if (userid == playerid)
	    return SendErrorMessage(playerid, "Você não pode enviar uma PM para si.");

	if (PlayerData[userid][pDisablePM])
	    return SendErrorMessage(playerid, "Este jogador desativou as mensagens privadas.");

	new last = GetPVarInt(playerid, GetLastPmString(userid));
	if (PlayerData[userid][pAdmin] > 0 && ((last + 60) < gettime()))
	{
		SetPVarInt(playerid, "pm_to", userid);
		SetPVarString(playerid, "pm_message", text);

		Dialog_Show(playerid, SendPrivateMessage, DIALOG_STYLE_MSGBOX, "Confirmação", "{C0C0C0}Você está prestes a enviar uma PM para um administrador. \nTenha em mente que os canais apropriados para obter suporte são {FFFFFF}/meajude e /reportar.{C0C0C0}\nA falha entender esse aviso poderá acarretar em punição para você.\n\n{F5DEB3}Tem certeza que deseja prosseguir?", "Eu Confirmo", "Cancelar");
		return 1;
	}

	SetPVarInt(playerid, GetLastPmString(userid), gettime());
	SendClientMessageEx(userid, COLOR_YELLOW, "(( PM de %s (%d): %s ))", ReturnName(playerid, 0, 1), playerid, text);
	SendClientMessageEx(playerid, COLOR_YELLOW, "(( PM para %s (%d): %s ))", ReturnName(userid, 0, 1), userid, text);
	return 1;
}

