	format(string, sizeof(string), "[Lixo %d]\n{FFFFFF}Capacidade: %d/20", id, GarbageData[id][garbageCapacity]);
  	UpdateDynamic3DTextLabelText(GarbageData[id][garbageText3D], COLOR_DARKBLUE, string);

  	SetPlayerAttachedObject(playerid, 4, 1264, 6, 0.000000, 0.000000, 0.000000, 0.000000, 270.000000, 90.000000, 0.500000, 0.500000, 0.500000);
	ShowPlayerFooter(playerid, "Aperte ~y~'N'~w~ para colocar no caminhao.");

	return 1;
}

CMD:despejarlixo(playerid, params[])
{
	new
		vehicleid = GetPlayerVehicleID(playerid),
		id = Job_NearestPoint(playerid, 5.0);

	if (PlayerData[playerid][pJob] != JOB_GARBAGE)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (GetVehicleModel(vehicleid) != 408)
	    return SendErrorMessage(playerid, "Você precisa estar dirigindo um caminhão de lixo.");

	if (id == -1 || JobData[id][jobType] != JOB_GARBAGE)
	    return SendErrorMessage(playerid, "Você não está próximo de um aterro sanitário.");

	if (CoreVehicles[vehicleid][vehTrash] < 1)
	    return SendErrorMessage(playerid, "Não há nenhum lixo dentro do caminhão.");

	GiveMoney(playerid, (CoreVehicles[vehicleid][vehTrash] * 25));
	ShowPlayerFooter(playerid, "Voce ~g~despejou~w~ o lixo!");

	SendServerMessage(playerid, "Voce ganhou $%d por largar %d sacolas de lixo.", (CoreVehicles[vehicleid][vehTrash] * 15), CoreVehicles[vehicleid][vehTrash]);
	CoreVehicles[vehicleid][vehTrash] = 0;

	return 1;
}

