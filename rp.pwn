		        SendServerMessage(playerid, "Você comprou o item soda de %s por $%d. Os itens foram adicionados ao inventário.", ReturnName(sellerid, 0), price);
		        SendServerMessage(sellerid, "%s comprou o hamburguer por $%d.", ReturnName(playerid, 0), price);
			}
			case 3:
		    {
				new id = Inventory_Add(playerid, "Hamburguer", 2703);

				if (id == -1)
				    return SendErrorMessage(playerid, "Você não tem mais slots vazios no inventário.");

		        SendServerMessage(playerid, "Você comprou o item hamburguer de %s por $%d. Os itens foram adicionados ao inventário.", ReturnName(sellerid, 0), price);
		        SendServerMessage(sellerid, "%s comprou o hamburguer por $%d.", ReturnName(playerid, 0), price);
