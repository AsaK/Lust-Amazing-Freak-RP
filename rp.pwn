CMD:escuta(playerid, params[]){
    if(GetFactionType(playerid) != FACTION_POLICE)
        return SendErrorMessage(playerid, "Você precisa ser policial para usar este comando.");
    if(isnull(params)){
        SendSyntaxMessage(playerid, "/escuta [opções]");
        SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} colocar, remover, removertodas");
        return 1;
    }
    static
        Float:fX,
        Float:fY,
        Float:fZ;

    GetPlayerPos(playerid, fX, fY, fZ);

    if(!strcmp(params, "colocar", true)){
        if(IsPlayerInAnyVehicle(playerid))
            return SendErrorMessage(playerid, "Você precisa sair do veículo.");
        if(!Inventory_HasItem(playerid, "Escuta"))
            return SendErrorMessage(playerid, "Você não possui uma escuta.");
        for(new i = 0; i < MAX_WIRES; i++) if(!WireData[i][wireExists])
        {
            WireData[i][wireExists] = true;
            WireData[i][wirePos][0] = fX;
            WireData[i][wirePos][1] = fY;
            WireData[i][wirePos][2] = fZ;

            new bubblestr[100];
            format(bubblestr, sizeof(bubblestr), "** %s coloca algo no chão.", ReturnName(playerid, 0));
            SetPlayerChatBubble(playerid, bubblestr, COLOR_PURPLE, 10.0, 6000);
            Inventory_Remove(playerid, "Escuta");
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s colocou uma escuta eletrônica em %s (ID: %d).", ReturnName(playerid, 0), GetLocation(fX, fY, fZ), i);
            return 1;
        }
        SendErrorMessage(playerid, "O servidor chegou ao limite de escutas spawnadas (/escuta removertodas).");
    }
    else if (!strcmp(params, "remover", true))
	{
        for (new i = 0; i < MAX_WIRES; i ++) if (WireData[i][wireExists] && IsPlayerInRangeOfPoint(playerid, 3.0, WireData[i][wirePos][0], WireData[i][wirePos][1], WireData[i][wirePos][2]))
	    {
            WireData[i][wireExists] = 0;
            foreach (new j : Player)
        	{
                if(PlayerData[j][pWireListen] == i){
                    PlayerData[j][pWireListen] = -1;
                    SendServerMessage(playerid, "Você parou de ouvir a escuta pois alguém retirou ela.");
                }
        	}
            new bubblestr[100];
            format(bubblestr, sizeof(bubblestr), "** %s remove uma escuta do chão.", ReturnName(playerid, 0));
            Inventory_Add(playerid, "Escuta", 1942);
            SetPlayerChatBubble(playerid, bubblestr, COLOR_PURPLE, 10.0, 6000);
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s retirou uma escuta eletrônica em %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));
			return 1;
		}
		SendErrorMessage(playerid, "Você não está próximo de nenhuma escuta.");
	}
	else if (!strcmp(params, "removertodas", true))
	{
        for (new i = 0; i < MAX_WIRES; i ++) if (WireData[i][wireExists])
	    {
            WireData[i][wireExists] = 0;
		}
		SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s destruiu todas as escutas eletrônicas.", ReturnName(playerid, 0));
	}
    return 1;
}

CMD:procurarescuta(playerid, params[]){
    if(GetFactionType(playerid) != FACTION_POLICE)
        return SendErrorMessage(playerid, "Você precisa ser policial para usar este comando.");
    if(isnull(params)){
        SendSyntaxMessage(playerid, "/procurarescuta [ID]");
        return 1;
    }
    new escuta = strval(params);
    if(escuta == -1) return 1;
    if(WireData[escuta][wireExists]){
        SetPlayerCheckpoint(playerid, WireData[escuta][wirePos][0], WireData[escuta][wirePos][1], WireData[escuta][wirePos][1], 1.0);
        SendServerMessage(playerid, "Um checkpoint foi criado no local da escuta ID %d", escuta);
    } else{
        SendErrorMessage(playerid, "Este ID de escuta não existe.");
    }
    return 1;
}

CMD:ouvirescuta(playerid, params[]){
    if(GetFactionType(playerid) != FACTION_POLICE)
        return SendErrorMessage(playerid, "Você precisa ser policial para usar este comando.");
    if(isnull(params)){
        SendSyntaxMessage(playerid, "/ouvirescuta [opções]");
        SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} ID(ouve a escuta), parar");
        return 1;
    }
    if(!strcmp(params, "parar", true)){
        if(IsPlayerInAnyVehicle(playerid))
            return SendErrorMessage(playerid, "Você precisa sair do veículo.");
        if(PlayerData[playerid][pWireListen] == -1)
            return SendErrorMessage(playerid, "Você não está ouvindo à nenhuma escuta.");
        SendServerMessage(playerid, "Você parou de ouvir a escuta ID %d.", PlayerData[playerid][pWireListen]);
        PlayerData[playerid][pWireListen] = -1;
        return 1;
    }
    new wid = strval(params);
    if(wid < 0 || strval(params) > MAX_WIRES)
        return SendErrorMessage(playerid, "ID da escuta inválido.");
    if(PlayerData[playerid][pWireListen] != -1)
        return SendErrorMessage(playerid, "Você já esta ouvindo a uma escuta. Utilize /ouvirescuta parar.");
    if(!WireData[wid][wireExists])
        return SendErrorMessage(playerid, "Esta escuta não existe.");
    PlayerData[playerid][pWireListen] = wid;
    SendServerMessage(playerid, "Você começou a ouvir a escuta ID %d", wid);
    return 1;
}

