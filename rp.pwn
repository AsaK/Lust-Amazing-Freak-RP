CMD:gmx(playerid, params[])
{
	new time;

	if (PlayerData[playerid][pAdmin] < 5)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (g_ServerRestart)
	{
	    TextDrawHideForAll(gServerTextdraws[3]);

	    g_ServerRestart = 0;
	    g_RestartTime = 0;

	    return SendClientMessageToAllEx(COLOR_LIGHTRED, "[ADMIN]: %s cancelou o GMX.", ReturnName(playerid, 0));
	}
	if (sscanf(params, "d", time))
	    return SendSyntaxMessage(playerid, "/gmx [segundos]");

	if (time < 3 || time > 600)
	    return SendErrorMessage(playerid, "Os segundos precisam estar entre 3 e 600.");

    TextDrawShowForAll(gServerTextdraws[3]);

	g_ServerRestart = 1;
	g_RestartTime = time;

	SendClientMessageToAllEx(COLOR_LIGHTRED, "[ADMIN]: Um GMX irá acontecer em %d segundos. Não é necessário deslogar de sua conta.", time);
	return 1;
}

