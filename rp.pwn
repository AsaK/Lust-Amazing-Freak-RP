CMD:blacklist(playerid, params[])
{
	static
		type[24];

    if (PlayerData[playerid][pAdmin] < 3)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "s[24]s[128]", type, params))
 	{
	 	SendSyntaxMessage(playerid, "/blacklist [nome] [opção]");
	 	SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} ban, banip, unbanip");
	 	return 1;
	}
	if (!strcmp(type, "ban", true))
	{
	    if (!IsValidPlayerName(params))
	        return SendErrorMessage(playerid, "O nome que você enviou não está no formato correto.");

		foreach (new i : Player) if (!strcmp(PlayerData[i][pUsername], params) || !strcmp(ReturnName(i), params, true)) {
		    KickEx(i);
		}
		Blacklist_Add("0.0.0.0", params, PlayerData[playerid][pUsername], "Name Ban (/blacklist)");

	    SendServerMessage(playerid, "Você baniu \"%s\".", params);
	    SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s baniu \"%s\".", ReturnName(playerid, 0), params);

	    Log_Write("logs/ban_log.txt", "[%s] %s baniu \"%s\".", ReturnDate(), ReturnName(playerid, 0), params);
	}
	else if (!strcmp(type, "banip", true))
	{
	    if (!IsAnIP(params))
	        return SendErrorMessage(playerid, "O IP que você digitou não está no formato correto");

		foreach (new i : Player) if (!strcmp(PlayerData[i][pIP], params)) {
		    KickEx(i);
		}
		Blacklist_Add(params, "", PlayerData[playerid][pUsername], "IP Ban (/blacklist)");

	    SendServerMessage(playerid, "Você baniu o IP \"%s\".", params);
	    SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s baniu o IP \"%s\".", ReturnName(playerid, 0), params);

	    Log_Write("logs/ban_log.txt", "[%s] %s baniu o IP \"%s\".", ReturnDate(), ReturnName(playerid, 0), params);
	}
	else if (!strcmp(type, "unbanip", true))
	{
	    if (!IsAnIP(params))
	        return SendErrorMessage(playerid, "O IP que você digitou não está no formato correto.");

		Blacklist_RemoveIP(params);

	    SendServerMessage(playerid, "Você desbaniu o IP \"%s\".", params);
	    SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s desbaniu o IP \"%s\".", ReturnName(playerid, 0), params);

	    Log_Write("logs/ban_log.txt", "[%s] %s desbaniu o IP \"%s\".", ReturnDate(), ReturnName(playerid, 0), params);
	}
	return 1;
}

