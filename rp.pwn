CMD:criarradar(playerid, params[])
{
	static
	    Float:limit,
	    Float:range;

	if (sscanf(params, "ff", limit, range))
		return SendSyntaxMessage(playerid, "/criarradar [limite] [range] (Range normal: 30)");

	if (limit < 5.0 || limit > 150.0)
	    return SendErrorMessage(playerid, "O limite precisa estar entre 5 e 150.");

	if (range < 5.0 || range > 50.0)
	    return SendErrorMessage(playerid, "A range precisa estar entre 5 e 50.");

	if (Speed_Nearest(playerid) != -1)
	    return SendErrorMessage(playerid, "Você não pode estar tão perto de outro radar.");

	new id = Speed_Create(playerid, limit, range);

	if (id == -1)
	    return SendErrorMessage(playerid, "O servidor chegou ao limite de radares.");

	SendServerMessage(playerid, "Você criou o radar ID: %d.", id);
	return 1;
}

