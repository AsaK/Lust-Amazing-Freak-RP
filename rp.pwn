CMD:setaradmin(playerid, params[])
{
	static
		userid,
	    level;

	if (PlayerData[playerid][pAdmin] < 6)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "ud", userid, level))
		return SendSyntaxMessage(playerid, "/setaradmin [playerid/nome] [level]");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "O ID é inválido.");

	if (level < 0 || level > 7)
	    return SendErrorMessage(playerid, "Level de administrador inválido. Leveis: 0-7.");

	if (level > PlayerData[userid][pAdmin])
	{
	    SendAdminAction(playerid, "Você promoveu %s ao level %d de administrador.", ReturnName(userid, 0), level);
	    SendAdminAction(userid, "%s promoveu você ao level %d de administrador.", ReturnName(playerid, 0), level);
	}
	else
	{
	    SendAdminAction(playerid, "Você rebaixou %s ao level %d de administrador.", ReturnName(userid, 0), level);
	    SendAdminAction(userid, "%s rebaixou você ao level %d de administrador.", ReturnName(playerid, 0), level);
	}
	PlayerData[userid][pAdmin] = level;
 	Log_Write("logs/admin_log.txt", "[%s] %s setou o admin level de %s para %d.", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0), level);

	return 1;
}

CMD:dardonate(playerid, params[])
{
	static
		userid,
	    level,
        days;

	if (PlayerData[playerid][pAdmin] < 6)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "udd", userid, level, days))
		return SendSyntaxMessage(playerid, "/dardonate [playerid/nome] [level] [dias]");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "O ID é inválido.");

	if (level < 0 || level > 3)
	    return SendErrorMessage(playerid, "Level de donate inválido. Leveis: 0-3.");

    if (days < 0 || days > 3)
	    return SendErrorMessage(playerid, "Número de Dias inválido. Dias: 1-30.");

    if(level == 0 || days == 0){
        SendAdminAction(playerid, "Você removeu o status de Donater de %s.", ReturnName(userid, 0));
        SendAdminAction(userid, "%s removeu o seu status de Donater.", ReturnName(playerid, 0));
        DonateData[userid][dLevel] = 0;
        DonateData[userid][dExpireTime] = 0;
        Log_Write("logs/admin_log.txt", "[%s] %s removeu o status de donater de %s.", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0));
    } else {
        DonateData[userid][dLevel] = level;
        DonateData[userid][dExpireTime] = gettime() + (86400*days);
        SQL_SaveAccount(userid);
    	SendAdminAction(playerid, "Você setou o nível %d de Donater para %s por %d dias.", level, ReturnName(userid, 0), days);
    	SendAdminAction(userid, "%s setou o seu nível de Donater para %d por %d dias.", ReturnName(playerid, 0), level, days);
     	Log_Write("logs/admin_log.txt", "[%s] %s setou o donate level de %s para %d por %d dias.", ReturnDate(), ReturnName(playerid, 0), ReturnName(userid, 0), level, days);
    }
	return 1;
}

