    DeletePVar(playerid, "HouseIDc");
    DeletePVar(playerid, "HousePricec");
    HouseData[hid][houseOwner] = 0;
    House_Refresh(hid);
    House_Save(hid);
    SendServerMessage(playerid, "Você vendeu a sua casa e recebeu {33CC33}$%d{FFFFFF}.", hprice);
    GiveMoney(playerid, hprice);
    return 1;
}

