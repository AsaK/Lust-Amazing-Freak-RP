CMD:pararcarregar(playerid, params[])
{
	if (PlayerData[playerid][pJob] != JOB_COURIER)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (!PlayerData[playerid][pLoading] && !PlayerData[playerid][pLoadType])
	    return SendErrorMessage(playerid, "Você não está carregando caixas no momento.");

	PlayerData[playerid][pLoading] = 0;
	PlayerData[playerid][pLoadType] = 0;
    PlayerData[playerid][pLoadCrate] = 0;

	RemovePlayerAttachedObject(playerid, 4);
	SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);

	DisablePlayerCheckpoint(playerid);
	SendServerMessage(playerid, "Você parou de carregar caixas.");

	return 1;
}

CMD:iniciarentrega(playerid, params[])
{
 	new id = Job_NearestPoint(playerid);

	if (PlayerData[playerid][pJob] != JOB_COURIER)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (id == -1 || JobData[id][jobType] != JOB_COURIER)
	    return SendErrorMessage(playerid, "Você não está próximo de um ponto de carregamento.");

	if (PlayerData[playerid][pLoadType] > 0)
	    return SendErrorMessage(playerid, "Você já iniciou uma entrega.");

    if (IsPlayerInAnyVehicle(playerid))
    	return SendErrorMessage(playerid, "Você precisa sair do veículo.");

	Dialog_Show(playerid, StartDelivery, DIALOG_STYLE_LIST, "Selecione o tipo", "24/7\nMunição\nRoupas\nSuprimentos de comida\nGasolina\nMóveis\nBebidas\nArtigos Eróticos\nBebidas Exóticas\nDonuts\nFichas e Bebidas\nIngredientes", "Selecionar", "Cancelar");
	return 1;
}

CMD:pedirentrega(playerid, params[])
{
    new id = (Business_Inside(playerid) == -1) ? (Business_Nearest(playerid)) : (Business_Inside(playerid));

    if (id == -1 || !Business_IsOwner(playerid, id))
        return SendErrorMessage(playerid, "Você não está perto da sua empresa.");

	if (BusinessData[id][bizType] == 5)
	    return SendErrorMessage(playerid, "Você não pode pedir entregas para esse tipo de empresa.");

	if (BusinessData[id][bizShipment])
	{
	    foreach (new i : Player) if (PlayerData[i][pShipment] == id)
		{
	        CancelShipment(i);
	        SendServerMessage(i, "O pedido de entregas foi cancelado.");
	    }
	    BusinessData[id][bizShipment] = 0;
	    Business_Save(id);

	    SendServerMessage(playerid, "Sua empresa não está mais pedindo entregas.");
	    SendJobMessage(1, COLOR_YELLOW, "** %s não está mais pedindo entregas para a empresa %s. **", ReturnName(playerid, 0), BusinessData[id][bizName]);
	}
	else
	{
	    if (BusinessData[id][bizDeliver][0] == 0.0 && BusinessData[id][bizDeliver][1] == 0.0 && BusinessData[id][bizDeliver][2] == 0.0)
	        return SendErrorMessage(playerid, "O ponto de entrega da sua empresa não está setado.");

	    BusinessData[id][bizShipment] = 1;
	    Business_Save(id);

	    SendServerMessage(playerid, "Você pediu uma entrega para sua empresa.");
		SendJobMessage(1, COLOR_YELLOW, "** %s precisa de uma entrega para a empresa %s! (/entregas para aceitar). **", ReturnName(playerid, 0), BusinessData[id][bizName]);
	}
	return 1;
}

CMD:cancelarentrega(playerid, params[])
{
	if (PlayerData[playerid][pJob] != JOB_COURIER)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

