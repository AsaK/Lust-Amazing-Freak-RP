CMD:boombox(playerid, params[])
{
	static
	    type[24],
	    string[128];

	if (!Inventory_HasItem(playerid, "Boombox"))
	    return SendErrorMessage(playerid, "Você não tem uma boombox.");

	if (sscanf(params, "s[24]S()[128]", type, string))
	{
	    SendSyntaxMessage(playerid, "/boombox [nome]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} colocar, pegar, url");
	    return 1;
	}
	if (!strcmp(type, "colocar", true))
	{
	    if (BoomboxData[playerid][boomboxPlaced])
	        return SendErrorMessage(playerid, "Você já colocou uma boombox.");

		if (Boombox_Nearest(playerid) != INVALID_PLAYER_ID)
		    return SendErrorMessage(playerid, "Você está muito perto de uma boombox.");

		if (IsPlayerInAnyVehicle(playerid))
		    return SendErrorMessage(playerid, "Você precisa sair do veículo.");

		Boombox_Place(playerid);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s liga e coloca uma boombox no chão.", ReturnName(playerid, 0));
		SendServerMessage(playerid, "Você colocou uma boombox no chão (Use \"/boombox\" para ver as opções).");
	}
	else if (!strcmp(type, "pegar", true))
	{
	    if (!BoomboxData[playerid][boomboxPlaced])
	        return SendErrorMessage(playerid, "Você não tem uma boombox spawnada.");

		if (!IsPlayerInRangeOfPoint(playerid, 3.0, BoomboxData[playerid][boomboxPos][0], BoomboxData[playerid][boomboxPos][1], BoomboxData[playerid][boomboxPos][2]))
		    return SendErrorMessage(playerid, "Você não está perto da sua boombox.");

		Boombox_Destroy(playerid);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega sua boombox.", ReturnName(playerid, 0));
	}
	else if (!strcmp(type, "url", true))
	{
	    if (sscanf(string, "s[128]", string))
	        return SendSyntaxMessage(playerid, "/boombox url [URL do som/rádio]");

        if (!BoomboxData[playerid][boomboxPlaced])
	        return SendErrorMessage(playerid, "Você não tem uma boombox spawnada.");

		if (!IsPlayerInRangeOfPoint(playerid, 3.0, BoomboxData[playerid][boomboxPos][0], BoomboxData[playerid][boomboxPos][1], BoomboxData[playerid][boomboxPos][2]))
		    return SendErrorMessage(playerid, "Você não está perto da sua boombox.");

		Boombox_SetURL(playerid, string);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s aperta um botão na boombox, alterado a rádio.", ReturnName(playerid, 0));
	}
	return 1;
}

CMD:adestruirboombox(playerid, params[])
{
    if (PlayerData[playerid][pAdmin] < 3)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	foreach (new i : Player) if (BoomboxData[i][boomboxPlaced] && IsPlayerInRangeOfPoint(playerid, 3.0, BoomboxData[i][boomboxPos][0], BoomboxData[i][boomboxPos][1], BoomboxData[i][boomboxPos][2])) {
		Boombox_Destroy(i);

		SendServerMessage(playerid, "Você removeu a boombox de %s.", ReturnName(i, 0));
		return SendServerMessage(i, "%s removeu sua boombox.", ReturnName(playerid, 0));
	}
	SendErrorMessage(playerid, "Você não está perto de nenhuma boombox.");
	return 1;
}

CMD:encontrarlixo(playerid, params[])
{
	if (PlayerData[playerid][pJob] != JOB_GARBAGE)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	new id = GetClosestGarbage(playerid);

	if (id == -1)
	    return SendErrorMessage(playerid, "Não há nenhuma lata de lixo disponível.");

	PlayerData[playerid][pCP] = 1;

	SetPlayerCheckpoint(playerid, GarbageData[id][garbagePos][0], GarbageData[id][garbagePos][1], GarbageData[id][garbagePos][2], 2.5);
	SendServerMessage(playerid, "A lata foi marcada no mapa.");
	return 1;
}

CMD:revistarilegais(playerid, params[])
{
	new userid;

	if (sscanf(params, "u", userid))
	{
		SendSyntaxMessage(playerid, "/revistarilegais [playerid/nome]");
		SendClientMessage(playerid, COLOR_YELLOW, "[DICA]:{FFFFFF} Use /revistarmochila para procurar na mochila.");
		return 1;
	}
	if (userid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "O jogador está offline ou longe de você.");

	if (Inventory_HasItem(userid, "Sementes de maconha"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Sementes de maconha (%d)", Inventory_Count(userid, "Sementes de maconha"));

    if (Inventory_HasItem(userid, "Sementes de cocaina"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Sementes de cocaína (%d)", Inventory_Count(userid, "Sementes de cocaina"));

    if (Inventory_HasItem(userid, "Sementes de heroina"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Sementes de heroína (%d)", Inventory_Count(userid, "Sementes de heroina"));

	if (Inventory_HasItem(userid, "Esteroides"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Esteroides (%d)", Inventory_Count(userid, "Esteroides"));

    if (Inventory_HasItem(userid, "Maconha"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Maconha (%d)", Inventory_Count(userid, "Maconha"));

    if (Inventory_HasItem(userid, "Cocaina"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Cocaina (%d)", Inventory_Count(userid, "Cocaina"));

    if (Inventory_HasItem(userid, "Heroina"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Heroina (%d)", Inventory_Count(userid, "Heroina"));

	for (new i = 0; i < 12; i ++) if (PlayerData[userid][pGuns][i] && PlayerData[userid][pAmmo][i] > 0) {
	    SendClientMessageEx(playerid, COLOR_LIGHTRED, "** %s", ReturnWeaponName(PlayerData[userid][pGuns][i]));
	}
	SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s começa a revistar %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
	return 1;
}

CMD:revistarmochila(playerid, params[])
{
    new userid, backpack;

	if (sscanf(params, "u", userid))
		return SendSyntaxMessage(playerid, "/revistarmochila [playerid/nome]");

	if (userid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "O jogador está offline ou longe de você.");

	if (!Inventory_HasItem(userid, "Mochila") || (backpack = GetPlayerBackpack(userid)) == -1)
	    return SendErrorMessage(playerid, "Este jogador não tem uma mochila.");

    if (Backpack_HasItem(backpack, "Sementes de maconha"))
		SendClientMessageEx(playerid, COLOR_LIGHTRED, "** Sementes de maconha (%d)", Backpack_Count(backpack, "Sementes de maconha"));

    if (Backpack_HasItem(backpack, "Sementes de cocaina"))
