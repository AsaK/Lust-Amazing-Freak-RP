    for(new i = 0; i < sizeof(g_aInventoryItems); i ++){
        if(!strcmp(g_aInventoryItems[i][e_InventoryItem], name, true)){
            SetPVarInt(playerid, "ItemID", g_aInventoryItems[i][e_InventoryModel]);
