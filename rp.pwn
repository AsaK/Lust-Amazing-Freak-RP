Dialog:mensais(playerid, response, listitem, inputtext[])
{
    if(!response){
        new string[60];
        format(string, sizeof(string), "Loja de Donates (%d uCoins)", DonateData[playerid][dCoins]);
        Dialog_Show(playerid, donate, DIALOG_STYLE_LIST, string, "Pacotes Mensais\nCustomizações\nBônus\nVeículos\nTeamSpeak", "Selecionar", "Fechar");
        return 1;
    }
    switch(listitem){
        case 0:{ //Pacote A
            if(DonateData[playerid][dCoins] < 15)
                return SendErrorMessage(playerid, "Você não possui uCoins suficientes para comprar isto!");
            if(DonateData[playerid][dLevel] >= 1)
                return SendErrorMessage(playerid, "Você já possui este nível de donate, ou um maior!");
            DonateData[playerid][dCoins] -= 15;
            DonateData[playerid][dLevel] = 1;
            DonateData[playerid][dNameC] ++;
            DonateData[playerid][dNumberC] ++;
            DonateData[playerid][dExpireTime] = gettime() + (86400*30);
            SendServerMessage(playerid, "Você comprou o Pacote A de Donate por um mês!");
        }
        case 1:{ //Pacote B
            if(DonateData[playerid][dCoins] < 25)
                return SendErrorMessage(playerid, "Você não possui uCoins suficientes para comprar isto!");
            if(DonateData[playerid][dLevel] >= 2)
                return SendErrorMessage(playerid, "Você já possui este nível de donate, ou um maior!");
            DonateData[playerid][dCoins] -= 25;
            DonateData[playerid][dLevel] = 2;
            DonateData[playerid][dNameC] += 2;
            DonateData[playerid][dNumberC] += 2;
            DonateData[playerid][dExpireTime] = gettime() + (86400*30);
            SendServerMessage(playerid, "Você comprou o Pacote B de Donate por um mês!");
        }
        case 2:{ //Pacote C
            if(DonateData[playerid][dCoins] < 35)
                return SendErrorMessage(playerid, "Você não possui uCoins suficientes para comprar isto!");
            if(DonateData[playerid][dLevel] >= 3)
                return SendErrorMessage(playerid, "Você já possui este nível de donate, ou um maior!");
            DonateData[playerid][dCoins] -= 35;
            DonateData[playerid][dLevel] = 3;
            DonateData[playerid][dNameC] += 3;
            DonateData[playerid][dNumberC] += 3;
            DonateData[playerid][dExpireTime] = gettime() + (86400*30);
            SendServerMessage(playerid, "Você comprou o Pacote C de Donate por um mês!");
        }
    }
    return 1;
}

