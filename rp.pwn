CMD:sacargov(playerid, params[])
{
	static
	    amount;

	if (GetFactionType(playerid) != FACTION_GOV)
	    return SendErrorMessage(playerid, "Você não é um membro do governo.");

	if (sscanf(params, "d", amount))
		return SendSyntaxMessage(playerid, "/sacargov [quantia] (Saldo: %s)", FormatNumber(g_TaxVault));

	if (!IsPlayerInCityHall(playerid))
	    return SendErrorMessage(playerid, "Você precisa estar na prefeitura para usar este comando.");

	if (amount < 1 || amount > g_TaxVault)
	    return SendErrorMessage(playerid, "Valor inválido.");

    if (PlayerData[playerid][pFactionRank] < FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1)
	    return SendErrorMessage(playerid, "Você precisa ser cargo %d.", FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1);

	Tax_AddMoney(-amount);

	GiveMoney(playerid, amount);
	SendServerMessage(playerid, "Você sacou %s do tesouro (Saldo: %s).", FormatNumber(amount), FormatNumber(g_TaxVault));

	SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s sacou %s do tesouro.", ReturnName(playerid, 0), FormatNumber(amount));
	Log_Write("logs/tax_vault.txt", "[%s] %s sacou %s do tesouro.", ReturnDate(), ReturnName(playerid, 0), FormatNumber(amount));
	return 1;
}

CMD:depositargov(playerid, params[])
{
