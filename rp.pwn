		    }
  			PlayerData[playerid][pDisableSpeedo] = 0;
     		SendServerMessage(playerid, "Você ativou o velocimetro.");
		}
	}
	return 1;
}

CMD:multar(playerid, params[])
{
	static
	    userid,
	    price,
	    reason[64];

	if (GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Você precisa ser um policial.");

	if (sscanf(params, "uds[64]", userid, price, reason))
		return SendSyntaxMessage(playerid, "/multar [playerid/nome] [valor] [motivo]");

	if (userid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "O jogador está offline ou longe de você.");

	if (userid == playerid)
	    return SendErrorMessage(playerid, "Você não pode escrever uma multa para você mesmo.");

	if (price < 1 || price > 1000)
	    return SendErrorMessage(playerid, "A multa precisa estar entre $1 e $1,000.");

	new id = Ticket_Add(userid, price, reason);

	if (id != -1) {
	    SendServerMessage(playerid, "Você multou %s por %s, motivo: %s", ReturnName(userid, 0), FormatNumber(price), reason);
	    SendServerMessage(userid, "%s multou você em %s, motivo: %s", ReturnName(playerid, 0), FormatNumber(price), reason);

	    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s escreve uma multa para %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
	    Log_Write("logs/ticket_log.txt", "[%s] %s - Multa de %s para %s, motivo: %s", ReturnDate(), ReturnName(playerid, 0), FormatNumber(price), ReturnName(userid, 0), reason);
	}
	else {
	    SendErrorMessage(playerid, "Este jogador já está no limite das multas Limite: %d.", MAX_PLAYER_TICKETS);
	}
	return 1;
}

