	    return SendErrorMessage(playerid, "Você precisa estar na prefeitura.");

	if (isnull(params))
	    return SendSyntaxMessage(playerid, "/changenumber [novo número]");

    new numb = strval(params);

    if(numb < 1000 || numb > 99999)
        return SendErrorMessage(playerid, "Escolha um número entre 1000 e 99999!");

    if(DonateData[playerid][dNumberC] == 0)
        return SendErrorMessage(playerid, "Você não possui nenhuma NumberChange restante!");

	static
	    query[128];

	format(query, sizeof(query), "SELECT `Username` FROM `characters` WHERE `Phone` = '%d'", numb);
	mysql_function_query(g_iHandle, query, true, "OnVerifyNumberChange", "dd", playerid, numb);

	return 1;
}


