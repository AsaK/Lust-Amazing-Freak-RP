                }
            }
        }
    }
    new str[128];
    format(str, sizeof(str), "[SERVER]:{FFFFFF} Aguarde %d segundos enquanto o veículo é destravado.", GetLockTime(CarData[id][carLock]));
	SendClientMessage(playerid, COLOR_SERVER, str);
	return 1;
}

