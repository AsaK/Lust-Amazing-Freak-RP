	if ((id < 0 || id >= MAX_FACTIONS) || !FactionData[id][factionExists])
	    return SendErrorMessage(playerid, "O ID da facção é inválido.");

	Faction_Delete(id);
	SendServerMessage(playerid, "Você destruiu com sucesso a facção ID: %d.", id);
	return 1;
}

CMD:editarfac(playerid, params[])
{
	static
	    id,
	    type[24],
	    string[128];

	if (PlayerData[playerid][pAdmin] < 5 && !PlayerData[playerid][pFactionMod])
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	SendSyntaxMessage(playerid, "/editarfac [id] [nome]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} nome, cor, tipo, skins, equipamentos, cargos, maxcargos, tipocaixa");
		return 1;
	}
	if ((id < 0 || id >= MAX_FACTIONS) || !FactionData[id][factionExists])
	    return SendErrorMessage(playerid, "O ID da facção é inválido.");

    if (!strcmp(type, "nome", true))
	{
	    new name[32];

	    if (sscanf(string, "s[32]", name))
	        return SendSyntaxMessage(playerid, "/editarfac [id] nome [novo nome]");

	    format(FactionData[id][factionName], 32, name);

	    Faction_Save(id);
		SendFactionAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o nome da facção ID: %d para \"%s\".", ReturnName(playerid, 0), id, name);
	}
	else if (!strcmp(type, "maxcargos", true))
	{
	    new ranks;

	    if (sscanf(string, "d", ranks))
	        return SendSyntaxMessage(playerid, "/editarfac [id] maxcargos [máximo de cargos]");

		if (ranks < 1 || ranks > 15)
		    return SendErrorMessage(playerid, "O número precisa estar entre 1 e 15.");

	    FactionData[id][factionRanks] = ranks;

	    Faction_Save(id);
		SendFactionAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o máximo de cargos da facção ID: %d para %d.", ReturnName(playerid, 0), id, ranks);
	}
    else if (!strcmp(type, "tipocaixa", true))
	{
	    new tipocaixa;

	    if (sscanf(string, "d", tipocaixa))
	        return SendSyntaxMessage(playerid, "/editarfac [id] tipocaixa [1 = Armas, 2 = Drogas]");

		if (tipocaixa < 1 || tipocaixa >2)
		    return SendErrorMessage(playerid, "O número precisa estar entre 1 e 2.");

	    FactionData[id][factionCType] = tipocaixa;

	    Faction_Save(id);
		SendFactionAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o tipo de caixas da facção ID: %d para %d.", ReturnName(playerid, 0), id, tipocaixa);
	}
	else if (!strcmp(type, "cargos", true))
	{
	    Faction_ShowRanks(playerid, id);
	}
	else if (!strcmp(type, "cor", true))
	{
	    new color;

	    if (sscanf(string, "h", color))
	        return SendSyntaxMessage(playerid, "/editarfac [id] [cor] [hex]");

	    FactionData[id][factionColor] = color;
	    Faction_Update(id);

	    Faction_Save(id);
		SendFactionAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou a {%06x}cor{FF6347} da facção ID: %d.", ReturnName(playerid, 0), color >>> 8, id);
	}
	else if (!strcmp(type, "tipo", true))
	{
	    new typeint;

	    if (sscanf(string, "d", typeint))
     	{
		 	SendSyntaxMessage(playerid, "/editarfac [id] tipo [tipo]");
            SendClientMessage(playerid, COLOR_YELLOW, "[TIPOS]:{FFFFFF} 1: Police | 2: News | 3: Medical | 4: Government | 5: Gang");
            return 1;
		}
		if (typeint < 1 || typeint > 5)
		    return SendErrorMessage(playerid, "Opção inválida. Tipos estão entre 1 e 5.");

	    FactionData[id][factionType] = typeint;

	    Faction_Save(id);
		SendFactionAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o tipo da facção ID: %d para %d.", ReturnName(playerid, 0), id, typeint);
	}
	else if (!strcmp(type, "skins", true))
	{
	    static
	        skins[8];

		for (new i = 0; i < sizeof(skins); i ++)
		    skins[i] = (FactionData[id][factionSkins][i]) ? (FactionData[id][factionSkins][i]) : (19300);

	    PlayerData[playerid][pFactionEdit] = id;
		ShowModelSelectionMenu(playerid, "Skins", MODEL_SELECTION_SKINS, skins, sizeof(skins), -16.0, 0.0, -55.0);
	}
	else if (!strcmp(type, "equipamentos", true))
	{
        PlayerData[playerid][pFactionEdit] = id;
		Dialog_Show(playerid, FactionLocker, DIALOG_STYLE_LIST, "Equipamentos", "Setar local\nSetar armas", "Selecionar", "Cancelar");
	}
	return 1;
}

