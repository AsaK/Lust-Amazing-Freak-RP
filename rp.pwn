}

CMD:setardistintivo(playerid, params[]){
    new
	    userid,badge;

	if (PlayerData[playerid][pFaction] == -1)
	    return SendErrorMessage(playerid, "Você precisa estar em uma facção.");

	if (PlayerData[playerid][pFactionRank] < FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1)
	    return SendErrorMessage(playerid, "Você precisa ser cargo %d.", FactionData[PlayerData[playerid][pFaction]][factionRanks] - 1);

	if (sscanf(params, "ud", userid, badge))
	    return SendSyntaxMessage(playerid, "/setardistintivo [playerid/nome] [Numero]");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "Este jogador está desconectado.");
    if(badge < 1 || badge > 999)
        return SendErrorMessage(playerid, "O distintivo deve estar entre 1 e 999.");

	if (PlayerData[userid][pFaction] != PlayerData[playerid][pFaction])
	    return SendErrorMessage(playerid, "Este jogador não está em sua facção.");

    new ftype = FactionData[PlayerData[playerid][pFaction]][factionType];
    if (ftype != FACTION_POLICE && ftype != FACTION_MEDIC && ftype != FACTION_GOV)
	    return SendErrorMessage(playerid, "Sua facção não possui acesso à isso.");
    if(PlayerData[userid][pBadge] != 0)
        return SendErrorMessage(playerid, "Este jogador já possui um distintivo.");

    SendServerMessage(playerid, "Você setou o distintivo de %s para #%d.", ReturnName(userid, 0), badge);
    Inventory_Add(userid, "Distintivo", 19347, 1);
    SendServerMessage(userid, "%s setou o seu distintivo para #%d.", ReturnName(playerid, 0), badge);
    PlayerData[userid][pBadge] = badge;
	return 1;
}

