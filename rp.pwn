Dialog:GraffitiText(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new id = Graffiti_Nearest(playerid);

		if (id == -1)
		    return 0;
	
	    if (isnull(inputtext))
	        return Dialog_Show(playerid, GraffitiText, DIALOG_STYLE_INPUT, "Texto do graffiti", "Digite o texto que você quer grafitar abaixo:\n\nOBS: O limite é de 64 caracteres.", "Enviar", "Cancelar");
		if (strlen(inputtext) > 64)
		    return Dialog_Show(playerid, GraffitiText, DIALOG_STYLE_INPUT, "Texto do graffiti", "ERRO: Seu texto excedeu 64 caracteres.\n\n\nDigite o texto que você quer grafitar abaixo:\n\nOBS: O limite é de 64 caracteres.", "Enviar", "Cancelar");
        if (IsSprayingInProgress(id))
	        return SendErrorMessage(playerid, "Um jogador já está grafitando este ponto.");

        PlayerData[playerid][pGraffiti] = id;
        PlayerData[playerid][pGraffitiTime] = 15;

		strpack(PlayerData[playerid][pGraffitiText], inputtext, 64 char);
		ApplyAnimationEx(playerid, "GRAFFITI", "spraycan_fire", 4.1, 1, 0, 0, 0, 0, 1);
		
		ShowPlayerFooter(playerid, "Agora voce esta pintando seu ~g~graffiti.");
		GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~b~Pintando...~w~ aguarde!", 15000, 3);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega uma lata de spray e começa a pintar a parede.", ReturnName(playerid, 0));
	}
	return 1;
}

Dialog:ConfirmHouse(playerid, response, listitem, inputtext[]){
