CMD:pegar(playerid, params[])
{
	new
	    userid,
		string[128];

    if (GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Você precisa ser um policial.");

	if (sscanf(params, "u", userid))
	    return SendSyntaxMessage(playerid, "/pegar [playerid/nome]");

    if (userid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "O jogador está offline ou longe de você.");

	if (GetWeaponCount(userid) > 0)
		strcat(string, "Pegar armas\n");

	if (Inventory_HasItem(userid, "Sementes de maconha") || Inventory_HasItem(userid, "Sementes de cocaina") || Inventory_HasItem(userid, "Sementes de heroina"))
	    strcat(string, "Pegar sementes\n");

	if (Inventory_HasItem(userid, "Maconha") || Inventory_HasItem(userid, "Cocaina") || Inventory_HasItem(userid, "Heroina") || Inventory_HasItem(userid, "Esteroides"))
	    strcat(string, "Pegar drogas\n");

    if (Inventory_HasItem(userid, "Radio"))
	    strcat(string, "Pegar radio\n");

	if (Inventory_HasItem(userid, "Mochila") && GetPlayerBackpack(userid) != -1)
	    strcat(string, "Pegar mochila\n");

    if (Inventory_HasItem(userid, "Porte de armas"))
	    strcat(string, "Pegar porte de armas\n");

   	if (Inventory_HasItem(userid, "Carteira de habilitação"))
	    strcat(string, "Pegar carteira de habilitação\n");

	if (!strlen(string))
	    return SendErrorMessage(playerid, "Não há nada para pegar.");

	PlayerData[playerid][pTakeItems] = userid;
	Dialog_Show(playerid, TakeItems, DIALOG_STYLE_LIST, "Pegar itens", string, "Pegar", "Cancelar");
	return 1;
}

CMD:arrombar(playerid, params[])
{
	static
	    id = -1;

	if (GetFactionType(playerid) != FACTION_POLICE)
	    return SendErrorMessage(playerid, "Você precisa ser um policial.");

	if ((id = House_Nearest(playerid)) != -1)
	{
	    if (!HouseData[id][houseLocked])
	        return SendErrorMessage(playerid, "Essa casa já está destrancada.");

	    ShowPlayerFooter(playerid, "Tentando ~r~arrombar~w~ a porta...");
	    ApplyAnimation(playerid, "POLICE", "Door_Kick", 4.0, 0, 0, 0, 0, 0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s tenta arrombar a porta.", ReturnName(playerid, 0));
	    SetTimerEx("KickHouse", 1500, false, "dd", playerid, id);
	}
	else if ((id = Business_Nearest(playerid)) != -1)
	{
		if (!BusinessData[id][bizLocked])
		    return SendErrorMessage(playerid, "Esta empresa já está destrancada.");

		ShowPlayerFooter(playerid, "Tentando ~r~arrombar~w~ a porta...");
        ApplyAnimation(playerid, "POLICE", "Door_Kick", 4.0, 0, 0, 0, 0, 0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s tenta arrombar a porta.", ReturnName(playerid, 0));
	    SetTimerEx("KickBusiness", 1500, false, "dd", playerid, id);
	}
	else {
		SendErrorMessage(playerid, "Você precisa estar próximo a uma casa ou empresa.");
	}
	return 1;
}

CMD:comprarestante(playerid, params[])
{
	new houseid = House_Inside(playerid);

	if (houseid == -1 || !House_IsOwner(playerid, houseid))
	    return SendErrorMessage(playerid, "Você precisa estar dentro da casa.");

	if (isnull(params) || (!isnull(params) && strcmp(params, "confirmar", true) != 0))
		return SendSyntaxMessage(playerid, "/comprarestante [confirmar] (Taxa: $1,000)");

	if (Rack_Count(playerid) >= 4)
	    return SendErrorMessage(playerid, "Sua casa só pode ter 4 estantes de armas na casa.");

	if (Rack_Nearest(playerid) != -1)
	    return SendErrorMessage(playerid, "Você não pode usar este comando próximo de outra estante de armas.");

	if (GetMoney(playerid) < 1000)
	    return SendErrorMessage(playerid, "Você não tem dinheiro suficiente para comprar isso.");

	new id = Rack_Create(playerid, houseid);

	if (id == -1)
	    return SendErrorMessage(playerid, "O serivodr chegou ao limite de estantes spawnadas.");

    ResetEditing(playerid);

	PlayerData[playerid][pEditRack] = id;
	EditDynamicObject(playerid, RackData[id][rackObjects][4]);

    GiveMoney(playerid, -1000);
	SendServerMessage(playerid, "Você comprou uma estante de armas por $1,000.");
	return 1;
}

