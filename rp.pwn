public OnPlayerCommandReceived(playerid, cmdtext[])
{
	if (!SQL_IsLogged(playerid) || (PlayerData[playerid][pTutorial] > 0 || PlayerData[playerid][pTutorialStage] > 0 || PlayerData[playerid][pKilled] > 0 || PlayerData[playerid][pHospital] != -1))
	    return 0;

	if (PlayerData[playerid][pMuted] && strfind(cmdtext, "/desmutar", true) != 0)
 	{
	    SendErrorMessage(playerid, "Você está mutado pelo servidor.");
	    return 0;
	}
	if (PlayerData[playerid][pCommandCount] < 6)
	{
	    PlayerData[playerid][pCommandCount]++;

	    if (PlayerData[playerid][pCommandCount] == 6) {
	        PlayerData[playerid][pCommandCount] = 0;

	        PlayerData[playerid][pMuted] = 1;
	        PlayerData[playerid][pMuteTime] = 5;

	        SendServerMessage(playerid, "Você foi mutado por spam (5 segundos).");
	        SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s foi mutado por spam.", ReturnName(playerid, 0));
	        return 0;
		}
	}
	return 1;
}

public OnPlayerText(playerid, text[])
{
	if ((!PlayerData[playerid][pLogged] && !PlayerData[playerid][pCharacter]) || PlayerData[playerid][pTutorial] > 0 || PlayerData[playerid][pTutorialStage] > 0 || PlayerData[playerid][pHospital] != -1)
	    return 0;

	if (PlayerData[playerid][pMuted])
	{
	    SendErrorMessage(playerid, "Você está mutado pelo servidor.");
	    return 0;
	}
	if (PlayerData[playerid][pSpamCount] < 5)
	{
	    PlayerData[playerid][pSpamCount]++;

	    if (PlayerData[playerid][pSpamCount] == 5) {
	        PlayerData[playerid][pSpamCount] = 0;

	        PlayerData[playerid][pMuted] = 1;
	        PlayerData[playerid][pMuteTime] = 5;

	        SendServerMessage(playerid, "Você foi mutado por spam (5 segundos).");
	        SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s foi mutado por spam.", ReturnName(playerid, 0));
	        return 0;
		}
	}
	if (PlayerData[playerid][pNewsGuest] != INVALID_PLAYER_ID && GetFactionType(PlayerData[playerid][pNewsGuest]) == FACTION_NEWS && IsPlayerInAnyVehicle(playerid) && IsNewsVehicle(GetPlayerVehicleID(playerid)))
	{
	    foreach (new i : Player) if (!PlayerData[i][pDisableBC]) {
	  		SendClientMessageEx(i, COLOR_LIGHTGREEN, "[NOTÍCIAS] Convidado %s: %s", ReturnName(playerid, 0), text);
		}
	   	return 0;
   	}
	else
	{
		new
			targetid = PlayerData[playerid][pCallLine];


        if (IsPlayerInAnyVehicle(playerid) && IsWindowedVehicle(GetPlayerVehicleID(playerid)) && !CoreVehicles[GetPlayerVehicleID(playerid)][vehWindowsDown])
			SendVehicleMessage(GetPlayerVehicleID(playerid), 0xBBFFEEFF, "[Veículo] %s diz: %s", ReturnName(playerid, 0), text);

		else
		{
            new string[200];
		    if (!IsPlayerOnPhone(playerid)){
                format(string,sizeof(string),"%s diz: %s", ReturnName(playerid, 0), text);
                ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
                for(new j = 0; j < MAX_WIRES; j++){
                    if(IsPlayerInRangeOfPoint(playerid, 20.0, WireData[j][wirePos][0], WireData[j][wirePos][1], WireData[j][wirePos][2])){
                        foreach(new i : Player){
                            if(PlayerData[i][pWireListen] == j){
                                new wirestr[128];
                                format(wirestr, sizeof(wirestr), "[Escuta] %s diz: %s", ReturnName(playerid, 0), text);
                                SendClientMessage(i, 0x93d680FF, wirestr);
                            }
                        }
                    }
                }
            }
			else{
                format(string,sizeof(string),"(Celular) %s diz: %s", ReturnName(playerid, 0), text);
                ProxDetector(20.0, playerid, string,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);

                for(new j = 0; j < MAX_WIRES; j++){
                    if(IsPlayerInRangeOfPoint(playerid, 20.0, WireData[j][wirePos][0], WireData[j][wirePos][1], WireData[j][wirePos][2])){
                        foreach(new i : Player){
                            if(PlayerData[i][pWireListen] == j){
                                new wirestr[128];
                                format(wirestr, sizeof(wirestr), "[Escuta] (Celular) %s diz: %s", ReturnName(playerid, 0), text);
                                SendClientMessage(i, 0x93d680FF, wirestr);
                            }
                        }
                    }
                }
            }

		}
		switch (PlayerData[playerid][pEmergency])
		{
			case 1:
			{
				if (!strcmp(text, "LSPD", true))
				{
				    PlayerData[playerid][pEmergency] = 2;
				    SendClientMessage(playerid, COLOR_LIGHTBLUE, "[Operador]:{FFFFFF} Você pediu pela LSPD, qual é a sua emergência?");
				}
				else if (!strcmp(text, "LSFD", true))
				{
				    PlayerData[playerid][pEmergency] = 3;
				    SendClientMessage(playerid, COLOR_HOSPITAL, "[Operador]:{FFFFFF} Você pediu pela LSFD, qual é a sua emergência?");
				}
				else SendClientMessage(playerid, COLOR_LIGHTBLUE, "[Operador]:{FFFFFF} Desculpe, não entendi. Você precisa da \"LSPD\" ou da \"LSFD\"?");
			}
			case 2:
			{
   				SendFactionMessageEx(FACTION_POLICE, COLOR_RADIO, "Central diz: %s ligou para a emergência do 10-20 %s.", ReturnName(playerid, 0), GetPlayerLocation(playerid));
        		SendFactionMessageEx(FACTION_POLICE, COLOR_RADIO, "Central diz: E reportou o seguinte: %s", text);

			    SendClientMessage(playerid, COLOR_LIGHTBLUE, "[Operador]:{FFFFFF} Nós avisamos todas as unidades da região.");
			    cmd_desligar(playerid, "\1");

			    SetFactionMarker(playerid, FACTION_POLICE, 0x00D700FF);
			}
			case 3:
			{
			    SendFactionMessageEx(FACTION_MEDIC, COLOR_HOSPITAL, "Central diz: %s ligou para a emergência do 10-20 %s.", ReturnName(playerid, 0), GetPlayerLocation(playerid));
       			SendFactionMessageEx(FACTION_MEDIC, COLOR_HOSPITAL, "Central diZ: E reportou o seguinte: %s", text);

                foreach(new i : Player){
                    if(IsPlayerInRangeOfPoint(i, 50.0, 1736.0386,-1109.3013,24.0859)){
                        new handle = Audio_Play(i, 4);
                        Audio_Set3DPosition(i, handle, 1736.0386,-1109.3013,24.0859, 30.0);
                    }
                }
			    SendClientMessage(playerid, COLOR_HOSPITAL, "[Operador]:{FFFFFF} Nós avisamos todas as unidades da região.");
			    cmd_desligar(playerid, "\1");

			    SetFactionMarker(playerid, FACTION_MEDIC, 0x00D700FF);
			}
		}
		switch (PlayerData[playerid][pPlaceAd])
		{
		    case 1:
		    {
			    if (!strcmp(text, "sim", true))
		        {
		            if (GetMoney(playerid) < 250)
				    {
    	                SendClientMessage(playerid, COLOR_CYAN, "[Operador]:{FFFFFF} Desculpe, você não tem dinheiro para anunciar.");
					    cmd_desligar(playerid, "\1");
					}
					else
					{
						PlayerData[playerid][pPlaceAd] = 2;
						SendClientMessage(playerid, COLOR_CYAN, "[Operador]:{FFFFFF} Por favor, especifique seu anúncio.");
					}
				}
			}
			case 2:
			{
                new price;
                switch(DonateData[playerid][dLevel]){
                    case 0: price = 250;
                    case 1: price = 125;
                    case 2: price = 0;
                    case 3: price = 0;
                }
			    if (GetMoney(playerid) < price)
			    {
                    SendClientMessage(playerid, COLOR_CYAN, "[Operador]:{FFFFFF} Desculpe, você não tem dinheiro para anunciar.");
				    cmd_desligar(playerid, "\1");
				}
				else
				{
				    GiveMoney(playerid, -price);
				    SetTimerEx("Advertise", 3000, false, "d", playerid);

                    PlayerData[playerid][pAdTime] = 120;
				    strpack(PlayerData[playerid][pAdvertise], text, 128 char);

        	        SendClientMessage(playerid, COLOR_CYAN, "[Operador]:{FFFFFF} Seu anúncio será publicado em breve.");
				    cmd_desligar(playerid, "\1");
				}
			}
		}
		if (targetid != INVALID_PLAYER_ID && !PlayerData[playerid][pIncomingCall])
		{
			SendClientMessageEx(targetid, COLOR_YELLOW, "(Celular) %s diz: %s", ReturnName(playerid, 0), text);
            foreach(new p : Player){
                if(PlayerData[p][pGrampeando] == PlayerData[playerid][pPhone] || PlayerData[p][pGrampeando] == PlayerData[targetid][pPhone]){
                    new gstr[128];
                    format(gstr, sizeof(gstr), "[Grampo] (Celular) %s diz: %s",ReturnName(playerid, 0), text);
                    SendClientMessage(p, 0x93d680FF, gstr);
                }
            }
		}
	}
	return 0;
}

public OnPlayerEditDynamicObject(playerid, objectid, response, Float:x, Float:y, Float:z, Float:rx, Float:ry, Float:rz)
{
