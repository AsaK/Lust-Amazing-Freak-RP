	return 1;
}

CMD:apinturav(playerid, params[])
{
	static
	    paintjobid;

    if (PlayerData[playerid][pAdmin] < 3)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (!IsPlayerInAnyVehicle(playerid))
	    return SendErrorMessage(playerid, "Você não está dentro de um veículo.");

	if (sscanf(params, "d", paintjobid))
	    return SendSyntaxMessage(playerid, "/apinturav [ID da paintjob] (-1 para desativar)");

	if (paintjobid < -1 || paintjobid > 5)
	    return SendErrorMessage(playerid, "A paintjob precisa estar entre -1 e 5.");

	if (paintjobid == -1)
		paintjobid = 6;

	SetVehiclePaintjob(GetPlayerVehicleID(playerid), paintjobid);
	SendServerMessage(playerid, "Você alterou o paintjob para %d.", paintjobid);
	return 1;
}

