CMD:cozinhar(playerid, params[])
{
	new houseid = House_Inside(playerid);

	if (houseid == -1)
	    return SendErrorMessage(playerid, "Você precisa estar dentro da casa para cozinhar algo.");

	if (isnull(params))
	    return SendSyntaxMessage(playerid, "/cozinhar [hamburguer/pizza]");

    if (PlayerData[playerid][pCuffed])
	    return SendErrorMessage(playerid, "Você não pode usar este comando agora.");

	if (!strcmp(params, "hamburguer", true))
	{
	    if (!Inventory_HasItem(playerid, "Hamburguer congelado"))
	        return SendErrorMessage(playerid, "Você não tem um hamburguer congelado no inventário.");

		if (PlayerData[playerid][pCooking])
		    return SendErrorMessage(playerid, "Você já está cozinhando.");

		Inventory_Add(playerid, "Papelão", 928);

		PlayerData[playerid][pCooking] = 1;
		PlayerData[playerid][pCookingTime] = 20;
		PlayerData[playerid][pCookingHouse] = houseid;

		Inventory_Remove(playerid, "Hamburguer congelado");
        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s abre o microondas, coloca o hamburguer congelado e aperta o botão de ligar (20 segundos).", ReturnName(playerid, 0));
	}
	else if (!strcmp(params, "pizza", true))
	{
	    if (!Inventory_HasItem(playerid, "Pizza congelada"))
	        return SendErrorMessage(playerid, "Você não tem uma pizza congelada no inventário.");

		if (PlayerData[playerid][pCooking])
		    return SendErrorMessage(playerid, "Você já está cozinhando.");

        Inventory_Add(playerid, "Papelão", 928);

		PlayerData[playerid][pCooking] = 2;
		PlayerData[playerid][pCookingTime] = 55;
		PlayerData[playerid][pCookingHouse] = houseid;

        Inventory_Remove(playerid, "Pizza congelada");
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s prepara o forno, coloca a pizza dentro e fecha a porta (55 segundos).", ReturnName(playerid, 0));
	}
	return 1;
}

