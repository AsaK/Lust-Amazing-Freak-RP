CMD:ir(playerid, params[])
{
	static
	    id,
	    type[24],
		string[64];

	if (PlayerData[playerid][pAdmin] < 1 && PlayerData[playerid][pTester] < 1)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "u", id))
 	{
	 	SendSyntaxMessage(playerid, "/ir [player ou opção]");
		SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} spawn, prisao, casa, empresa, entrada, emprego, portao, interior, banner");
		return 1;
	}
    if (id == INVALID_PLAYER_ID)
	{
	    if (sscanf(params, "s[24]S()[64]", type, string))
		{
		    SendSyntaxMessage(playerid, "/ir [player ou opção]");
			SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} spawn, prisao, casa, empresa, entrada, emprego, portao, interior, banner");
			return 1;
	    }
	    if (!strcmp(type, "spawn", true)) {
	        SetDefaultSpawn(playerid);

	        return SendServerMessage(playerid, "Você foi teleportado para o spawn principal.");
		}
		else if (!strcmp(type, "prisao", true))
		{
	        SetPlayerPos(playerid, 283.5930, 1413.3511, 10.4078);
	        SetPlayerFacingAngle(playerid, 180.0000);

	        SetPlayerInterior(playerid, 0);
	        SetPlayerVirtualWorld(playerid, 0);

	        return SendServerMessage(playerid, "Você foi teleportado para a prisão.");
		}
		//
		else if (!strcmp(type, "banner", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir banner [ID do banner]");

			if ((id < 0 || id >= MAX_BILLBOARDS) || !BillBoardData[id][bbExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de anúncio.");

		    SetPlayerPos(playerid, BillBoardData[id][bbPos][0], BillBoardData[id][bbPos][1], BillBoardData[id][bbPos][2]);
		    SendServerMessage(playerid, "Você foi teleportado para o anúncio de ID %d.", id);
		    return 1;
		}
		//
		else if (!strcmp(type, "casa", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir [casa] [ID da casa]");

			if ((id < 0 || id >= MAX_HOUSES) || !HouseData[id][houseExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de casa.");

		    SetPlayerPos(playerid, HouseData[id][housePos][0], HouseData[id][housePos][1], HouseData[id][housePos][2]);
		    SetPlayerInterior(playerid, HouseData[id][houseExterior]);

			SetPlayerVirtualWorld(playerid, HouseData[id][houseExteriorVW]);
		    SendServerMessage(playerid, "Você foi teleportado para a casa de ID %d.", id);
		    return 1;
		}
		else if (!strcmp(type, "empresa", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir [empresa] [ID da empresa]");

			if ((id < 0 || id >= MAX_BUSINESSES) || !BusinessData[id][bizExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de empresa.");

		    SetPlayerPos(playerid, BusinessData[id][bizPos][0], BusinessData[id][bizPos][1], BusinessData[id][bizPos][2]);
		    SetPlayerInterior(playerid, BusinessData[id][bizExterior]);

			SetPlayerVirtualWorld(playerid, BusinessData[id][bizExteriorVW]);
		    SendServerMessage(playerid, "Você foi teleportado para a empresa de ID %d.", id);
		    return 1;
		}
		else if (!strcmp(type, "entrada", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir [entrada] [ID da entrada]");

			if ((id < 0 || id >= MAX_ENTRANCES) || !EntranceData[id][entranceExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de entrada.");

		    SetPlayerPos(playerid, EntranceData[id][entrancePos][0], EntranceData[id][entrancePos][1], EntranceData[id][entrancePos][2]);
		    SetPlayerInterior(playerid, EntranceData[id][entranceExterior]);

			SetPlayerVirtualWorld(playerid, EntranceData[id][entranceExteriorVW]);
		    SendServerMessage(playerid, "Você foi teleportado para a entrada de ID %d.", id);
		    return 1;
		}
		else if (!strcmp(type, "emprego", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir [emprego] [ID do emprego]");

			if ((id < 0 || id >= MAX_DYNAMIC_JOBS) || !JobData[id][jobExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de emprego");

		    SetPlayerPos(playerid, JobData[id][jobPos][0], JobData[id][jobPos][1], JobData[id][jobPos][2]);
		    SetPlayerInterior(playerid, JobData[id][jobInterior]);

			SetPlayerVirtualWorld(playerid, JobData[id][jobWorld]);
		    SendServerMessage(playerid, "Você foi teleportado para o emprego de ID: %d.", id);
		    return 1;
		}
		else if (!strcmp(type, "portao", true))
		{
		    if (sscanf(string, "d", id))
		        return SendSyntaxMessage(playerid, "/ir [portao] [ID do portao]");

			if ((id < 0 || id >= MAX_GATES) || !GateData[id][gateExists])
			    return SendErrorMessage(playerid, "Você especificou um ID inválido de portão.");

		    SetPlayerPos(playerid, GateData[id][gatePos][0] - (2.5 * floatsin(-GateData[id][gatePos][3], degrees)), GateData[id][gatePos][1] - (2.5 * floatcos(-GateData[id][gatePos][3], degrees)), GateData[id][gatePos][2]);
		    SetPlayerInterior(playerid, GateData[id][gateInterior]);

			SetPlayerVirtualWorld(playerid, GateData[id][gateWorld]);
		    SendServerMessage(playerid, "Você foi teleportado para o portão de ID %d.", id);
		    return 1;
		}
		else if (!strcmp(type, "interior", true))
		{
		    static
		        str[1536];

			str[0] = '\0';

			for (new i = 0; i < sizeof(g_arrInteriorData); i ++) {
			    strcat(str, g_arrInteriorData[i][e_InteriorName]);
			    strcat(str, "\n");
		    }
		    Dialog_Show(playerid, TeleportInterior, DIALOG_STYLE_LIST, "TP: Lista de interiores", str, "Selecionar", "Cancelar");
		    return 1;
		}
	    else return SendErrorMessage(playerid, "O ID é inválido.");
	}
	if (!IsPlayerSpawned(id))
		return SendErrorMessage(playerid, "Você não pode teleportar para um personagem não spawnado.");

	SendPlayerToPlayer(playerid, id);

	format(string, sizeof(string), "Voce ~y~teleportou para %s.", ReturnName(id, 0));
	ShowPlayerFooter(playerid, string);

	return 1;
}

