CMD:largarmochila(playerid, params[])//NÃO TEM NO /AJUDA
{
	new
		id = GetPlayerBackpack(playerid);

	if (!Inventory_HasItem(playerid, "Mochila") || id == -1)
	    return SendErrorMessage(playerid, "Você não tem uma mochila no inventário.");

	if (!Backpack_GetItems(id))
	    return SendErrorMessage(playerid, "Você não pode largar uma mochila vazia.");

    if (IsPlayerInAnyVehicle(playerid) || !IsPlayerSpawned(playerid))
    	return SendErrorMessage(playerid, "Você não pode largar uma mochila agora");

	static
	    Float:x,
	    Float:y,
	    Float:z;

	GetPlayerPos(playerid, x, y, z);

	BackpackData[id][backpackPlayer] = 0;
    BackpackData[id][backpackPos][0] = x;
    BackpackData[id][backpackPos][1] = y;
    BackpackData[id][backpackPos][2] = z;
    BackpackData[id][backpackInterior] = GetPlayerInterior(playerid);
    BackpackData[id][backpackWorld] = GetPlayerVirtualWorld(playerid);

	Backpack_Refresh(id);
	Backpack_Save(id);

	Inventory_Remove(playerid, "Mochila");
	SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s larga uma mochila no chão.", ReturnName(playerid, 0));

	SetAccessories(playerid);

	return 1;
}

CMD:setaritem(playerid, params[])
{
	static
	    userid,
		item[32],
		amount;

	if (PlayerData[playerid][pAdmin] < 4)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "uds[32]", userid, amount, item))
	    return SendSyntaxMessage(playerid, "/setaritem [playerid/nome] [quantia] [nome do item]");

    if (IsFurnitureItem(item))
	{
	    for (new i = 0; i < sizeof(g_aFurnitureData); i ++) if (!strcmp(g_aFurnitureData[i][e_FurnitureName], item, true))
		{
	        Inventory_Set(userid, g_aFurnitureData[i][e_FurnitureName], g_aFurnitureData[i][e_FurnitureModel], amount);

			SendServerMessage(playerid, "Você deu para %s o item \"%s\", %d.", ReturnName(userid, 0), item, amount);
			return 1;
		}
	}
	else for (new i = 0; i < sizeof(g_aInventoryItems); i ++) if (!strcmp(g_aInventoryItems[i][e_InventoryItem], item, true))
	{
	    if (!strcmp(item, "Celular", true)) {
	        PlayerData[userid][pPhone] = random(90000) + 10000;
	    }
        Inventory_Set(userid, g_aInventoryItems[i][e_InventoryItem], g_aInventoryItems[i][e_InventoryModel], amount);

		return SendServerMessage(playerid, "Você deu para %s o item \"%s\", %d.", ReturnName(userid, 0), item, amount);
	}
	SendErrorMessage(playerid, "Item inválido (Use /listaitens para ver a lista).");
	return 1;
}

CMD:recuperarveiculo(playerid, params[])
{
