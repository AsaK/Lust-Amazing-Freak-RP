	        return SendErrorMessage(playerid, "Você precisa ter duas gramas de heorina para usar.");

        PlayerData[playerid][pDrugTime] = 30;
		PlayerData[playerid][pDrugUsed] = 3;
        SetPlayerArmour(playerid, 70);

		Inventory_Remove(playerid, "Heroina", 2);

		ApplyAnimation(playerid, "SMOKING", "M_smk_in", 4.1, 0, 0, 0, 0, 0, 1);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega um pouco de heroina na seringa e injeta.", ReturnName(playerid, 0));
	}
	else if (!strcmp(params, "esteroides", true))
	{
	    if (!Inventory_HasItem(playerid, "Esteroides"))
	        return SendErrorMessage(playerid, "Você precisa de uma pílula de esteroide para usar.");

