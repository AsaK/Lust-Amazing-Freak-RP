CMD:compraralarme(playerid, params[])
{
	new vehicleid = GetPlayerVehicleID(playerid);
    new level;
    if(sscanf(params, "d", level))
        return SendSyntaxMessage(playerid, "/compraralarme [Level] | 1 = $1000, 2 = $3000");
    if(level < 1 || level > 2)
        return SendErrorMessage(playerid, "Os níveis vão de 1 à 2 apenas.");
	if (!IsEngineVehicle(vehicleid))
		return SendErrorMessage(playerid, "Você não está dentro de um veículo.");

	if (GetPlayerState(playerid) != PLAYER_STATE_DRIVER)
	    return SendErrorMessage(playerid, "Você precisa ser o motorista para usar este comando.");
    new price;
    switch(level){
        case 1: price = 1000;
        case 2: price = 3000;
    }
    new carid;
    if ((carid = Car_Inside(playerid)) != -1 && Car_IsOwner(playerid, carid)){
        if(CarData[carid][carAlarm] >= level)
            return SendErrorMessage(playerid, "Você já possui este alarme ou um melhor!");
        if(GetMoney(playerid) < price)
            return SendErrorMessage(playerid, "Você não possui dinheiro suficiente para fazer isto!");
        CarData[carid][carAlarm] = level;
        GiveMoney(playerid, -price);
        SendServerMessage(playerid, "Você comprou um alarme para o seu veículo!");
    } else{
        SendErrorMessage(playerid, "Você não é o dono deste veículo!");
    }
	return 1;
}

