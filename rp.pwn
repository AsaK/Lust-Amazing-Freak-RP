CMD:v(playerid, params[])
{
    if (isnull(params))
	{
	    SendSyntaxMessage(playerid, "/v [Parâmetro]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} estacionar, trancar, abandonar, removermods, portamalas, lista, luzes, capo");
		return 1;
	}
	if(!strcmp(params, "estacionar", true)) cmd_estacionar(playerid, "\1");
    else if(!strcmp(params, "trancar", true)) cmd_trancar(playerid, "\1");
    else if(!strcmp(params, "abandonar", true)) cmd_abandonar(playerid, "\1");
    else if(!strcmp(params, "removermods", true)) cmd_removermods(playerid, "\1");
    else if(!strcmp(params, "portamalas", true)) cmd_portamalas(playerid, "\1");
    else if(!strcmp(params, "lista", true)) cmd_meusveiculos(playerid, "\1");
    else if(!strcmp(params, "luzes", true)) cmd_luzes(playerid, "\1");
    else if(!strcmp(params, "capo", true)) cmd_capo(playerid, "\1");
    return 1;
}

CMD:nomeempresa(playerid, params[])
{
	new
