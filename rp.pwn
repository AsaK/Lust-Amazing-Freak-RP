public OnPlayerCommandPerformed(playerid, cmdtext[], success)
{
    if(!success){
        new cmdstr[100];
        format(cmdstr, sizeof(cmdstr), "{FFFF90}[SERVER] {FFCC00}(%s){FFFFFF} não é um comando interno válido!", cmdtext);
        SendClientMessage(playerid, COLOR_SERVER, cmdstr);
    }
    return 1;
}

