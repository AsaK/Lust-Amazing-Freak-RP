	PlayerData[userid][pFactionOffer] = playerid;
    PlayerData[userid][pFactionOffered] = PlayerData[playerid][pFaction];

    SendServerMessage(playerid, "Você pediu para %s entrar na facção \"%s\".", ReturnName(userid, 0), Faction_GetName(playerid));
    SendServerMessage(userid, "%s ofereceu a você uma vaga na facção \"%s\" (Use \"/aceitar faccao\").", ReturnName(playerid, 0), Faction_GetName(playerid));

