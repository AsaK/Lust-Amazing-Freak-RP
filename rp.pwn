	return 1;
}

CMD:apreender(playerid, params[])
{
	new
		price,
		id = Impound_Nearest(playerid),
		vehicleid = GetPlayerVehicleID(playerid),
		already = 0;

    if (GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Você precisa ser um policial.");

    if (sscanf(params, "d", price))
        return SendSyntaxMessage(playerid, "/apreender [valor]");

	if (price < 1 || price > 1000)
	    return SendErrorMessage(playerid, "O preço precisa estar entre $1,000 e $1.");

	if (GetVehicleModel(vehicleid) != 525)
	    return SendErrorMessage(playerid, "Você precisa estar dirigindo um tow truck.");

	if (id == -1)
	    return SendErrorMessage(playerid, "Você não está em um pátio de apreensão.");

	if (!GetVehicleTrailer(vehicleid))
	    return SendErrorMessage(playerid, "Não há nenhum veículo conectado ao guincho.");

 	vehicleid = Car_GetID(GetVehicleTrailer(vehicleid));

	if (vehicleid == -1)
	    return SendErrorMessage(playerid, "Você não pode guinchar este veículo.");

	if (CarData[vehicleid][carImpounded] != -1)
		already = 1;
	//     return SendErrorMessage(playerid, "Este veículo já está apreendido.");

	CarData[vehicleid][carImpounded] = ImpoundData[id][impoundID];
	CarData[vehicleid][carImpoundPrice] = price;

	if(!already) Tax_AddMoney(price);

	GetVehiclePos(CarData[vehicleid][carVehicle], CarData[vehicleid][carPos][0], CarData[vehicleid][carPos][1], CarData[vehicleid][carPos][2]);
	Car_Save(vehicleid);

	SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s apreendeu o %s por %s.", ReturnName(playerid, 0), ReturnVehicleModelName(CarData[vehicleid][carModel]), FormatNumber(price));
 	DetachTrailerFromVehicle(GetPlayerVehicleID(playerid));

	return 1;
}

CMD:rfreq(playerid, params[])
{
	new channel;

	if (!Inventory_HasItem(playerid, "Radio"))
	    return SendErrorMessage(playerid, "Você precisa ter um rádio portátil.");

	if (sscanf(params, "d", channel))
 	{
	 	SendSyntaxMessage(playerid, "/rfreq [frequência] (0 para desativar)");

	 	if (PlayerData[playerid][pChannel] > 0)
			SendClientMessageEx(playerid, COLOR_YELLOW, "[OBS]:{FFFFFF} Sua frequência está setada em %d.", PlayerData[playerid][pChannel]);

		return 1;
	}
	if (channel < 0 || channel > 999999)
	    return SendErrorMessage(playerid, "O canal não pode ser menor que 0 ou maior que 999,999.");
    if(channel == 911 && GetFactionType(playerid) != FACTION_POLICE)
        return SendErrorMessage(playerid, "Você não tem permissão para acessar esta frequência.");
    if(channel == 912 && GetFactionType(playerid) != FACTION_MEDIC)
        return SendErrorMessage(playerid, "Você não tem permissão para acessar esta frequência.");
    if(channel == 913 && GetFactionType(playerid) != FACTION_GOV)
        return SendErrorMessage(playerid, "Você não tem permissão para acessar esta frequência.");

	PlayerData[playerid][pChannel] = channel;

	if (channel == 0)
	    SendServerMessage(playerid, "Você desativou seu rádio.");

	else SendServerMessage(playerid, "Você setou a frequência do seu rádio para %d (\"/r [texto]\" para usar).", channel);
	return 1;
}

CMD:r(playerid, params[])
{
	if (isnull(params))
	    return SendSyntaxMessage(playerid, "/r [texto]");

    if (!Inventory_HasItem(playerid, "Radio"))
	    return SendErrorMessage(playerid, "Você precisa ter um rádio portátil.");

	if (!PlayerData[playerid][pChannel])
	    return SendErrorMessage(playerid, "Seu rádio está desativado (/rfreq).");

	static
	    string[128], string2[200];
	if(PlayerData[playerid][pChannel] == 911 && GetFactionType(playerid) != FACTION_POLICE) return SendErrorMessage(playerid, "This is police department's freqency only.");
	if (strlen(params) > 64)
	{
		format(string, sizeof(string), "** [RADIO: %d] %s: %.64s",PlayerData[playerid][pChannel],ReturnName(playerid, 0), params);
		SendRadioMessage(PlayerData[playerid][pChannel], COLOR_SERVER, string);
		format(string, sizeof(string), "...%s **",params[64]);
		SendRadioMessage(PlayerData[playerid][pChannel], COLOR_SERVER, string);

        format(string2,sizeof(string2),"** (Radio) %s: %.64s", ReturnName(playerid, 0), params);
        ProxDetectorN(20.0, playerid, string2,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
        format(string2,sizeof(string2),"...%s **", params[64]);
        ProxDetectorN(20.0, playerid, string2,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);

	}
	else {
		format(string, sizeof(string),"** [RADIO: %d] %s: %s **", PlayerData[playerid][pChannel],ReturnName(playerid, 0), params);
		SendRadioMessage(PlayerData[playerid][pChannel], COLOR_SERVER, string);

        format(string2,sizeof(string2),"** (Radio) %s: %.64s", ReturnName(playerid, 0), params);
        ProxDetectorN(20.0, playerid, string2,COLOR_FADE1,COLOR_FADE2,COLOR_FADE3,COLOR_FADE4,COLOR_FADE5);
	}
    foreach(new i : Player){
        if(IsPlayerNearPlayer(i, playerid, 20.0)) Play3dAudio(i, playerid, 6);
    }
	return 1;
}

