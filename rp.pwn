Dialog:CheckPerson(playerid, response, listitem, inputtext[])
{
	if (GetFactionType(playerid) != FACTION_POLICE || !IsACruiser(GetPlayerVehicleID(playerid)))
	    return 0;

	if (response)
	{
	    static
			targetid = INVALID_PLAYER_ID;


		if ((targetid = GetPlayerID(inputtext, 0)) == INVALID_PLAYER_ID)
		    return SendErrorMessage(playerid, "Este jogador não está conectado.");

        new stringx[512], address[32], Vehicles[128];
        format(Vehicles, sizeof(Vehicles), "  - Nenhum");
        format(address, sizeof(address), "Não possui");
        for(new i = 0; i < MAX_HOUSES; i++){
            if(House_IsOwner(targetid, i)){
                format(address, sizeof(address), "%s", HouseData[i][houseAddress]);
