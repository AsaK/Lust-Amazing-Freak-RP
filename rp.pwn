Dialog:Inventory(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    new
			itemid = PlayerData[playerid][pInventoryItem],
			string[64];

	    strunpack(string, InventoryData[playerid][itemid][invItem]);

	    switch (listitem)
	    {
	        case 0:
	        {
	            if (!strcmp(string, "Demo Soda") && PlayerData[playerid][pTutorialStage] == 3)
			    {
        			PlayerData[playerid][pThirst] = 100;
        			Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Tutorial", "Você aprendeu a pegar (do chão) e usar itens.\nComo você deve ter notado, ao beber sua sede passou de 80 para 100 no indicador.\n\nOs indicadores de fome e sede aparecem logo abaixo do dinheiro.\nSe sua fome ou sede chegar a zero, seu personagem vai começar a perder vida.", "Continuar", "");
			    }
			    else
			    {
		            CallLocalFunction("OnPlayerUseItem", "dds", playerid, itemid, string);
				}
	        }
	        case 1:
	        {
	            if (!strcmp(string, "Demo Soda"))
	                return 0;

				PlayerData[playerid][pInventoryItem] = itemid;
				Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Dar item", "Digite o nome ou o ID do jogador que você deseja dar o item:", "Dar", "Cancelar");
	        }
	        case 2:
	        {
	            new id = -1;

	            if (!strcmp(string, "Demo Soda") && PlayerData[playerid][pTutorialStage] != 4)
	                return 0;

				if (PlayerData[playerid][pTutorialStage] == 4)
				{
					Inventory_Remove(playerid, "Demo Soda");
					Dialog_Show(playerid, Tutorial, DIALOG_STYLE_MSGBOX, "Tutorial", "Você largou a garrafa no chão. Você pode pegar itens do chão apertando a tecla 'N'.\nVocê também pode trocar ou dar itens para outros jogadores.\n\nAs casas e portamalas veiculares podem servir para guardar itens.\nO inventario do seu personagem pode conter um máximo de %d itens únicos.", "Continuar", "", MAX_INVENTORY);
					return 1;
				}
	            if (IsPlayerInAnyVehicle(playerid) || !IsPlayerSpawned(playerid))
	                return SendErrorMessage(playerid, "Você não pode largar itens agora.");

				else if (!strcmp(string, "Mochila"))
					return cmd_largarmochila(playerid, "\1");

				else if ((id = Garbage_Nearest(playerid)) != -1)
				{
				    if (GarbageData[id][garbageCapacity] >= 20)
				        return SendErrorMessage(playerid, "Essa lata de lixo está cheia.");

                    GarbageData[id][garbageCapacity]++;
                    Garbage_Save(id);

					Inventory_Remove(playerid, string);
                    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s joga o item \"%s\" para dentro do lixo.", ReturnName(playerid, 0), string);

                    format(string, sizeof(string), "[Lixo %d]\n{FFFFFF}Capacidade: %d/20", id, GarbageData[id][garbageCapacity]);
                    UpdateDynamic3DTextLabelText(GarbageData[id][garbageText3D], COLOR_DARKBLUE, string);
				}
				else if (InventoryData[playerid][itemid][invQuantity] == 1)
					DropPlayerItem(playerid, itemid);

				else
					Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Largar item", "Item: %s - Quantidade: %d\n\nDigite a quantia que você deseja largar no chão:", "Largar", "Cancelar", string, InventoryData[playerid][itemid][invQuantity]);
	        }
	    }
	}
	return 1;
}

Dialog:DropItem(playerid, response, listitem, inputtext[])
{
	new
	    itemid = PlayerData[playerid][pInventoryItem],
	    string[32];

	strunpack(string, InventoryData[playerid][itemid][invItem]);

	if (response)
	{
	    if (isnull(inputtext))
	        return Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Largar item", "Item: %s - Quantidade: %d\n\nDigite a quantia que você deseja largar no chão:", "Largar", "Cancelar", string, InventoryData[playerid][itemid][invQuantity]);

		if (strval(inputtext) < 1 || strval(inputtext) > InventoryData[playerid][itemid][invQuantity])
		    return Dialog_Show(playerid, DropItem, DIALOG_STYLE_INPUT, "Largar item", "Erro: Você não tem tudo isso.\n\nItem: %s - Quantidade: %d\n\nDigite a quantia que você deseja largar no chão:", "Largar", "Cancelar", string, InventoryData[playerid][itemid][invQuantity]);

		DropPlayerItem(playerid, itemid, strval(inputtext));
	}
	return 1;
}

Dialog:BusinessBuy(playerid, response, listitem, inputtext[])
{
	static
	    bizid = -1,
		price,
		string[64];

    if ((bizid = Business_Inside(playerid)) != -1 && response)
    {
        price = BusinessData[bizid][bizPrices][listitem];

        if (GetMoney(playerid) < price)
            return SendErrorMessage(playerid, "Você não tem dinheiro suficiente para comprar essa isso.");

		if (BusinessData[bizid][bizProducts] < 1)
		    return SendErrorMessage(playerid, "Esta empresa está sem estoque.");

		if (BusinessData[bizid][bizType] == 1 || BusinessData[bizid][bizType] == 6)
		{
		    switch (listitem)
		    {
		        case 0:
		        {
		            if (Inventory_HasItem(playerid, "Celular"))
		                return SendErrorMessage(playerid, "Você já tem um celular.");

					new id = Inventory_Add(playerid, "Celular", 330);

					if (id == -1)
        				return SendErrorMessage(playerid, "Você não tem slots disponíveis no inventário.");

					PlayerData[playerid][pPhone] = random(90000) + 10000;

					GiveMoney(playerid, -price);
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s paga %s e pega uma sacola com um celular dentro.", ReturnName(playerid, 0), FormatNumber(price));

					BusinessData[bizid][bizProducts]--;
					BusinessData[bizid][bizVault] += Tax_Percent(price);

					Business_Save(bizid);
					Tax_AddPercent(price);

					format(string, sizeof(string), "Seu número é ~p~%d.", PlayerData[playerid][pPhone]);
					ShowPlayerFooter(playerid, string);

					SendServerMessage(playerid, "Seu novo número é %d.", PlayerData[playerid][pPhone]);
				}
				case 1:
		        {
		            if (Inventory_HasItem(playerid, "GPS"))
		                return SendErrorMessage(playerid, "Você já tem um GPS.");

					new id = Inventory_Add(playerid, "GPS", 18875);

					if (id == -1)
        				return SendErrorMessage(playerid, "Você não tem slots disponíveis no inventário.");

					GiveMoney(playerid, -price);
					SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s paga %s e pega uma sacola com um GPS dentro.", ReturnName(playerid, 0), FormatNumber(price));

					BusinessData[bizid][bizProducts]--;
					BusinessData[bizid][bizVault] += Tax_Percent(price);

					Business_Save(bizid);
					Tax_AddPercent(price);
			    }
				case 2:
		        {
		            if (Inventory_Count(playerid, "Lata de spray") >= 3)
		                return SendErrorMessage(playerid, "Você já tem 3 latas de spray no inventário.");

					new id = Inventory_Add(playerid, "Lata de spray", 365);

					if (id == -1)
