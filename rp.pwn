CMD:ligar(playerid, params[])
{
    if (!Inventory_HasItem(playerid, "Celular") || PlayerData[playerid][pPhone] == 0)
	    return SendErrorMessage(playerid, "Você não tem um celular.");

    if (PlayerData[playerid][pPhoneOff])
		return SendErrorMessage(playerid, "O celular precisa estar ligado.");

    if (PlayerData[playerid][pHospital] != -1 || PlayerData[playerid][pCuffed] || PlayerData[playerid][pInjured] || !IsPlayerSpawned(playerid))
	    return SendErrorMessage(playerid, "Você não pode usar este comando agora.");

	static
	    targetid,
		number;

	if (sscanf(params, "d", number))
 	   return SendSyntaxMessage(playerid, "/ligar [celular] (1222 para taxi, 911 para emergência, 222 para anúncios, 223 para banners)");

	if (!number)
	    return SendErrorMessage(playerid, "O número está fora de serviço.");

	if (number == 911)
	{
		PlayerData[playerid][pEmergency] = 1;
		PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega seu celular e faz uma ligação.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_LIGHTBLUE, "[OPERADOR]:{FFFFFF} Qual serviço você precisa, \"LSPD\" ou \"LSFD\"?");
	}
	else if (number == 1222)
	{
	    PlayerData[playerid][pTaxiCalled] = 1;
	    PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega seu celular e faz uma ligação.", ReturnName(playerid, 0));
		SendClientMessage(playerid, COLOR_YELLOW, "[OPERADOR]:{FFFFFF} A empresa de taxi foi avisada.");

        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s desliga a ligação.", ReturnName(playerid, 0));
		SendJobMessage(3, COLOR_YELLOW, "** %s precisa de um taxi em %s (Use /aceitarchamada para aceitar).", ReturnName(playerid, 0), GetPlayerLocation(playerid));
	}
	else if (number == 222)
	{
	    PlayerData[playerid][pPlaceAd] = 1;
	    PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega seu celular e faz uma ligação.", ReturnName(playerid, 0));

		if (PlayerData[playerid][pPlayingHours] < 4) {
            SendClientMessage(playerid, COLOR_CYAN, "[ERRO]:{FFFFFF} Você precisa jogar 4 horas para enviar um anúncio.");
		    cmd_desligar(playerid, "\1");
		}
		else if (PlayerData[playerid][pAdTime] < 1 && DonateData[playerid][dLevel] == 0) {
			SendClientMessage(playerid, COLOR_CYAN, "[OPERADOR]:{FFFFFF} Diga \"sim\" se você quer anunciar por $250.");
		}
        else if (PlayerData[playerid][pAdTime] < 1 && DonateData[playerid][dLevel] == 1) {
			SendClientMessage(playerid, COLOR_CYAN, "[OPERADOR]:{FFFFFF} Diga \"sim\" se você quer anunciar por $125 (Desconto de Donate).");
		}
        else if (PlayerData[playerid][pAdTime] < 1 && DonateData[playerid][dLevel] == 2) {
			SendClientMessage(playerid, COLOR_CYAN, "[OPERADOR]:{FFFFFF} Diga \"sim\" se você quer anunciar por $0 (Desconto de Donate).");
		}
        else if (PlayerData[playerid][pAdTime] < 1 && DonateData[playerid][dLevel] == 3) {
			SendClientMessage(playerid, COLOR_CYAN, "[OPERADOR]:{FFFFFF} Diga \"sim\" se você quer anunciar por $0 (Desconto de Donate).");
		}
		else {
		    SendClientMessage(playerid, COLOR_CYAN, "[ERRO]:{FFFFFF} Você já anunciou recentemente, aguarde um pouco.");
		    cmd_desligar(playerid, "\1");
		}
	}
	else if (number == 223)
	{
	    PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega seu celular e faz uma ligação.", ReturnName(playerid, 0));

		if (PlayerData[playerid][pPlayingHours] < 4) {
            SendClientMessage(playerid, COLOR_CYAN, "[ERRO]:{FFFFFF} Você precisa jogar 4 horas para publicar um banner.");
		    cmd_desligar(playerid, "\1");
		}
		SendClientMessageEx(playerid, COLOR_YELLOW, "[OPERADOR]:{FFFFFF} Olá, bem-vindo ao ramal da agência de publicidade de Los Santos, ouça as opções!");
		ViewBillboards(playerid);
	}
	else if ((targetid = GetNumberOwner(number)) != INVALID_PLAYER_ID)
	{
	    if (targetid == playerid)
	        return SendErrorMessage(playerid, "Você não pode ligar para você mesmo!");

		if (PlayerData[targetid][pPhoneOff])
		    return SendErrorMessage(playerid, "O celular está desligado.");

		PlayerData[targetid][pIncomingCall] = 1;
		PlayerData[playerid][pIncomingCall] = 1;

		PlayerData[targetid][pCallLine] = playerid;
		PlayerData[playerid][pCallLine] = targetid;

		SendClientMessageEx(playerid, COLOR_YELLOW, "[CELULAR]:{FFFFFF} Discando para #%d, aguarde uma resposta...", number);
		SendClientMessageEx(targetid, COLOR_YELLOW, "[CELULAR]:{FFFFFF} Ligação de #%d (Use \"/atender\" para atender).", PlayerData[playerid][pPhone]);

        PlayerPlaySound(playerid, 3600, 0.0, 0.0, 0.0);
        PlayerPlaySoundEx(targetid, 23000);

		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega seu celular e faz uma ligação.", ReturnName(playerid, 0));
	}
	else
	{
	    SendErrorMessage(playerid, "O número está fora de serviço.");
	}
	return 1;
}

