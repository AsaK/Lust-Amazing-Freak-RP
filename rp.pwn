CMD:arrombarveiculo(playerid, params[])
{
	new id = Car_Nearest(playerid);
	
	if (!Inventory_HasItem(playerid, "Pé de cabra"))
	    return SendErrorMessage(playerid, "Você não tem um pé de cabra.");
	
	if (id == -1)
	    return SendErrorMessage(playerid, "Você não está próximo de nenhum veículo.");
	    
	if (!CarData[id][carLocked])
	    return SendErrorMessage(playerid, "Este veículo não está trancado.");

	PlayerData[playerid][pPicking] = 1;
	PlayerData[playerid][pPickCar] = id;

	SendNearbyMessage(playerid, 20.0, COLOR_PURPLE, "** %s pega um pé de cabra e tenta abrir a porta do veículo.", ReturnName(playerid, 0));
    if(CarData[id][carAlarm] > 0){
        SendNearbyMessage(playerid, 20.0, COLOR_PURPLE, "* O alarme do veículo dispara. (( %s ))", ReturnName(playerid, 0));
        new engine, lights, alarm, doors, bonnet, boot, objective;
        GetVehicleParamsEx(CarData[id][carVehicle], engine, lights, alarm, doors, bonnet, boot, objective);
        SetVehicleParamsEx(CarData[id][carVehicle], engine, lights, 1, doors, bonnet, boot, objective);
        CarData[id][carAlarmTime] = 1;
        switch(CarData[id][carAlarm]){
            case 1:{
                for(new i = 0; i < MAX_PLAYERS; i++){
                    if(Car_IsOwner(i, id))
                        SendClientMessage(i, COLOR_YELLOW, "[SMS]: O alarme de seu veículo disparou - (Alarme Sys)");
                    break;
                }
            }
            case 2:{
                for(new i = 0; i < MAX_PLAYERS; i++){
                    if(Car_IsOwner(i, id))
                        SendClientMessage(i, COLOR_YELLOW, "[SMS]: O alarme de seu veículo disparou - (Alarme Sys)");
                    if(GetFactionType(i) == FACTION_POLICE && PlayerData[i][pOnDuty]){
                        new Float:fX, Float:fY, Float:fZ;
                        GetVehiclePos(CarData[id][carVehicle], fX, fY, fZ);
                        SendClientMessageEx(i, COLOR_RADIO, "[Alarme] O alarme de um %s disparou em %s", ReturnVehicleModelName(CarData[id][carModel]), GetLocation(fX, fY, fZ));
