Dialog:GiveItem(playerid, response, listitem, inputtext[])
{
	if (response)
	{
	    static
	        userid = -1,
			itemid = -1,
			string[32];

		if (sscanf(inputtext, "u", userid))
		    return Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Dar item", "Digite o nome ou o ID do jogador que receberá o item:", "Enviar", "Cancelar");

		if (userid == INVALID_PLAYER_ID)
		    return Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Dar item", "Erro: Jogador inválido.\n\nDigite o nome ou o ID do jogador que receberá o item:", "Enviar", "Cancelar");

	    if (!IsPlayerNearPlayer(playerid, userid, 6.0))
			return Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Dar item", "Erro: Você não está próximo do jogador.\n\nDigite o nome ou o ID do jogador que receberá o item:", "Enviar", "Cancelar");

	    if (userid == playerid)
			return Dialog_Show(playerid, GiveItem, DIALOG_STYLE_INPUT, "Dar item", "Erro: Você não pode entregar itens para você mesmo.\n\nDigite o nome ou o ID do jogador que receberá o item:", "Enviar", "Cancelar");

		itemid = PlayerData[playerid][pInventoryItem];

		if (itemid == -1)
		    return 0;

		strunpack(string, InventoryData[playerid][itemid][invItem]);

		if (InventoryData[playerid][itemid][invQuantity] == 1)
		{
			if (!strcmp(string, "Mochila") && Inventory_HasItem(userid, "Mochila"))
			    return SendErrorMessage(playerid, "Este jogador já tem uma mochila.");

		    new id = Inventory_Add(userid, string, InventoryData[playerid][itemid][invModel]);

		    if (id == -1)
				return SendErrorMessage(playerid, "Este jogador está sem slots livres no inventário.");

			if (!strcmp(string, "Mochila") && (id = GetPlayerBackpack(playerid)) != -1)
			{
			    BackpackData[id][backpackPlayer] = PlayerData[userid][pID];
				Backpack_Save(id);

				SetAccessories(userid);
			    Inventory_Remove(playerid, "Mochila");
			}
		    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega o item \"%s\" e entrega para %s.", ReturnName(playerid, 0), string, ReturnName(userid, 0));
		    SendServerMessage(userid, "%s deu para você um \"%s\". Os itens foram adicionados ao inventário.", ReturnName(playerid, 0), string);

			Inventory_Remove(playerid, string);
		    Log_Write("logs/give_log.txt", "[%s] %s (%s) entregou o item %s para %s (%s).", ReturnDate(), ReturnName(playerid, 0), PlayerData[playerid][pIP], string, ReturnName(userid, 0), PlayerData[userid][pIP]);
  		}
		else
		{
		    Dialog_Show(playerid, GiveQuantity, DIALOG_STYLE_INPUT, "Dar item", "Item: %s (Quantidade no seu inventário: %d)\n\nDigite a quantidade que você deseja entregar ao jogador %s:", "Entregar", "Cancelar", string, InventoryData[playerid][itemid][invQuantity], ReturnName(userid, 0));
		    PlayerData[playerid][pGiveItem] = userid;
		}
	}
	return 1;
}

