					return SendErrorMessage(playerid, "Você precisa ter 2 horas jogadas.");

				GiveWeaponToPlayer(playerid, DroppedItems[id][droppedWeapon], DroppedItems[id][droppedAmmo]);

				Item_Delete(id);
