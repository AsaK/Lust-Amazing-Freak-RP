    new day, month, year;
    getdate(year, month, day);
    format(PlayerData[playerid][pCardEmission], 11, "%d/%d/%d", day, month, year);
    format(PlayerData[playerid][pCardValid], 11, "%d/%d/%d", day, month, year+2);
    new SSN = randomEx(1000000, 9999999);
    PlayerData[playerid][pCardID] = SSN;
    static idquery[130];
    format(idquery, sizeof(idquery), "SELECT * FROM `characters` WHERE `SSN` = %d", SSN);
