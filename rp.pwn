	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} colocar, remover, removertodos");
		return 1;
	}
	static
        Float:fX,
        Float:fY,
        Float:fZ,
        Float:fA;

    GetPlayerPos(playerid, fX, fY, fZ);
    GetPlayerFacingAngle(playerid, fA);

	if (!strcmp(params, "colocar", true))
	{
	    if (IsPlayerInAnyVehicle(playerid))
	        return SendErrorMessage(playerid, "Você precisa sair do veículo.");

	    for (new i = 0; i != MAX_BARRICADES; i ++) if (!BarricadeData[i][cadeExists])
	    {
            BarricadeData[i][cadeExists] = true;
            BarricadeData[i][cadeType] = 1;

            BarricadeData[i][cadePos][0] = fX;
            BarricadeData[i][cadePos][1] = fY;
            BarricadeData[i][cadePos][2] = fZ;

            BarricadeData[i][cadeObject] = CreateDynamicObject(2899, fX, fY, fZ - 0.8, 0.0, 0.0, fA + 90.0);

			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s coloca um tapete de pregos no chão.", ReturnName(playerid, 0));
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s colocou um tapete de pregos em %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));
			return 1;
		}
		SendErrorMessage(playerid, "O servidor chegou ao limite de tapetes de pregos spawnados (/pregos removertodos).");
	}
	else if (!strcmp(params, "remover", true))
	{
        for (new i = 0; i != MAX_BARRICADES; i ++) if (BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 1 && IsPlayerInRangeOfPoint(playerid, 3.0, BarricadeData[i][cadePos][0], BarricadeData[i][cadePos][1], BarricadeData[i][cadePos][2]))
	    {
            BarricadeData[i][cadeExists] = 0;
            BarricadeData[i][cadeType] = 0;

            DestroyDynamicObject(BarricadeData[i][cadeObject]);

            SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s remove um tapete de pregos.", ReturnName(playerid, 0));
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s retirou um tapete de pregos em %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));
			return 1;
		}
		SendErrorMessage(playerid, "Você não está próximo de nenhum tapete de pregos");
	}
	else if (!strcmp(params, "removertodos", true))
	{
        for (new i = 0; i != MAX_BARRICADES; i ++) if (BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 1)
	    {
            BarricadeData[i][cadeExists] = 0;
            BarricadeData[i][cadeType] = 0;

			DestroyDynamicObject(BarricadeData[i][cadeObject]);
		}
		SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s destruiu todos os tapetes de prego.", ReturnName(playerid, 0));
	}
	return 1;
}

CMD:barreira(playerid, params[])
{
	if (GetFactionType(playerid) != FACTION_POLICE && GetFactionType(playerid) != FACTION_MEDIC)
	    return SendErrorMessage(playerid, "Você precisa ser policial para usar este comando.");
    new opt[15],brtype;
    if(sscanf(params,"s[15]D(0)", opt, brtype))
 	{
	 	SendSyntaxMessage(playerid, "/barreira [opção]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} colocar, remover, removertodas");
		return 1;
	}
	static
        Float:fX,
        Float:fY,
        Float:fZ,
        Float:fA;

    GetPlayerPos(playerid, fX, fY, fZ);
    GetPlayerFacingAngle(playerid, fA);

	if (!strcmp(opt, "colocar", true))
	{
	    if (IsPlayerInAnyVehicle(playerid))
	        return SendErrorMessage(playerid, "Você precisa sair do veículo.");
        if(brtype < 1 || brtype > 10)
            return SendErrorMessage(playerid, "As barricadas vão de 1 à 11 apenas.");
	    for(new i = 0; i != MAX_BARRICADES; i ++) if (!BarricadeData[i][cadeExists])
	    {
            BarricadeData[i][cadeExists] = true;
            BarricadeData[i][cadeType] = 2;

            BarricadeData[i][cadePos][0] = fX;
            BarricadeData[i][cadePos][1] = fY;
            BarricadeData[i][cadePos][2] = fZ;

            switch(brtype){
                case 1: BarricadeData[i][cadeObject] = CreateDynamicObject(1238, fX, fY, fZ, 0.0, 0.0, fA);
                case 2: BarricadeData[i][cadeObject] = CreateDynamicObject(978, fX, fY, fZ, 0.0, 0.0, fA);
                case 3: BarricadeData[i][cadeObject] = CreateDynamicObject(1228, fX, fY, fZ, 0.0, 0.0, fA);
                case 4: BarricadeData[i][cadeObject] = CreateDynamicObject(1237, fX, fY, fZ, 0.0, 0.0, fA);
                case 5: BarricadeData[i][cadeObject] = CreateDynamicObject(1282, fX, fY, fZ, 0.0, 0.0, fA);
                case 6: BarricadeData[i][cadeObject] = CreateDynamicObject(1422, fX, fY, fZ, 0.0, 0.0, fA);
                case 7: BarricadeData[i][cadeObject] = CreateDynamicObject(1423, fX, fY, fZ, 0.0, 0.0, fA);
                case 8: BarricadeData[i][cadeObject] = CreateDynamicObject(1424, fX, fY, fZ, 0.0, 0.0, fA);
                case 9: BarricadeData[i][cadeObject] = CreateDynamicObject(1434, fX, fY, fZ, 0.0, 0.0, fA);
                case 10: BarricadeData[i][cadeObject] = CreateDynamicObject(3091, fX, fY, fZ, 0.0, 0.0, fA);
                case 11: BarricadeData[i][cadeObject] = CreateDynamicObject(1437, fX, fY, fZ, 0.0, 0.0, fA);
            }
            SetPlayerPos(playerid, fX + 2, fY + 2, fZ + 2);
            EditDynamicObject(playerid, BarricadeData[i][cadeObject]);
            PlayerData[playerid][pEditingBarricade] = i;

			SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s coloca uma barreira na rua.", ReturnName(playerid, 0));
			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s colocou uma barreira em %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));

			return 1;
		}
		SendErrorMessage(playerid, "O servidor chegou ao limite de barreiras (/barreira removertados).");
	}
	else if (!strcmp(opt, "remover", true))
	{
        for (new i = 0; i != MAX_BARRICADES; i ++)
	    {
            if(BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 2 && IsPlayerInRangeOfPoint(playerid, 5.0, BarricadeData[i][cadePos][0], BarricadeData[i][cadePos][1], BarricadeData[i][cadePos][2])){
                BarricadeData[i][cadeExists] = 0;
                BarricadeData[i][cadeType] = 0;

                DestroyDynamicObject(BarricadeData[i][cadeObject]);

                SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s remove uma barreira.", ReturnName(playerid, 0));
    			SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s retirou uma barreira em %s.", ReturnName(playerid, 0), GetLocation(fX, fY, fZ));
            }
			return 1;
		}
		SendErrorMessage(playerid, "Você não está perto de nenhuma barreira.");
	}
	else if (!strcmp(opt, "removertodas", true))
	{
        for (new i = 0; i != MAX_BARRICADES; i ++) if (BarricadeData[i][cadeExists] && BarricadeData[i][cadeType] == 2)
	    {
            BarricadeData[i][cadeExists] = 0;
            BarricadeData[i][cadeType] = 0;

			DestroyDynamicObject(BarricadeData[i][cadeObject]);
		}
		SendFactionMessage(PlayerData[playerid][pFaction], COLOR_RADIO, "RADIO: %s destruiu todas as barreiras.", ReturnName(playerid, 0));
	}
	return 1;
}

CMD:changenumber(playerid, params[])
{
