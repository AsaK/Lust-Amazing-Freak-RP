CMD:algemar(playerid, params[])
{
    new
	    userid;

	if (GetFactionType(playerid) != FACTION_POLICE)
		return SendErrorMessage(playerid, "Você precisa ser um policial.");

	if (sscanf(params, "u", userid))
	    return SendSyntaxMessage(playerid, "/algemar [playerid/nome]");

	if (userid == INVALID_PLAYER_ID)
	    return SendErrorMessage(playerid, "Este jogador está offline.");

    if (userid == playerid)
	    return SendErrorMessage(playerid, "Você não pode se algemar.");

	if (!IsPlayerNearPlayer(playerid, userid, 5.0))
	    return SendErrorMessage(playerid, "Você precisa estar perto do jogador.");

    if (PlayerData[userid][pStunned] < 0 && GetPlayerSpecialAction(userid) != SPECIAL_ACTION_HANDSUP && !IsPlayerIdle(userid))
	    return SendErrorMessage(playerid, "O jogador precisa estar parado ou atordoado (tazer/bala de borracha).");

	if (GetPlayerState(userid) != PLAYER_STATE_ONFOOT)
	    return SendErrorMessage(playerid, "Você precisa estar a pé para algemar.");

    if (PlayerData[userid][pCuffed])
        return SendErrorMessage(playerid, "O jogador já está algemado.");
    if(!Inventory_HasItem(playerid, "Algemas"))
        return SendErrorMessage(playerid, "Você não possui algemas com você.");

	static
	    string[64];

	if (PlayerData[userid][pDrinking])
	{
        SetPlayerSpecialAction(playerid, SPECIAL_ACTION_NONE);

		DestroyProgressBar(PlayerData[userid][pDrinkBar]);
		PlayerData[userid][pDrinking] = 0;
	}
	if (PlayerData[userid][pHoldWeapon] > 0)
	{
	    HoldWeapon(userid, 0);
	}
    PlayerData[userid][pCuffed] = 1;
    SetPlayerSpecialAction(userid, SPECIAL_ACTION_CUFFED);
    Inventory_Remove(playerid, "Algemas");

	format(string, sizeof(string), "Voce foi ~r~algemado~w~ por %s.", ReturnName(playerid, 0));
    ShowPlayerFooter(userid, string);

    SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s pega um par de algemas e coloca-as nas mãos de %s.", ReturnName(playerid, 0), ReturnName(userid, 0));
    return 1;
}

