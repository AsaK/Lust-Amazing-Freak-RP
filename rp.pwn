CMD:editarbanner(playerid, params[])
{
	static
	    id,
	    type[24],
	    string[128];

	if (PlayerData[playerid][pAdmin] < 5)
	    return SendErrorMessage(playerid, "Você não pode usar este comando.");

	if (sscanf(params, "ds[24]S()[128]", id, type, string))
 	{
	 	SendSyntaxMessage(playerid, "/editarbanner [id] [nome]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} local, nome, valor, mensagem, dono, removerdono, range");
		return 1;
	}
	if ((id < 0 || id >= MAX_BILLBOARDS) || !BillBoardData[id][bbExists])
	    return SendErrorMessage(playerid, "O ID especificado é inválido.");

	if (!strcmp(type, "local", true))
	{
 		GetPlayerPos(playerid, BillBoardData[id][bbPos][0], BillBoardData[id][bbPos][1], BillBoardData[id][bbPos][2]);

		Billboard_Refresh(id);
		Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s ajustou o local do banner ID: %d.", ReturnName(playerid, 0), id);
	}
	else if (!strcmp(type, "valor", true))
	{
	    new price;

	    if (sscanf(string, "d", price))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] price [novo preço]");

	    BillBoardData[id][bbPrice] = price;

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o valor do banner ID: %d para %s.", ReturnName(playerid, 0), id, FormatNumber(price));
	}
	else if (!strcmp(type, "nome", true))
	{
	    new name[32];

	    if (sscanf(string, "s[32]", name))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] nome [novo nome]");

	    format(BillBoardData[id][bbName], 32, name);

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o valor do banner ID: %d para \"%s\".", ReturnName(playerid, 0), id, name);
	}
	else if (!strcmp(type, "message", true))
	{
	    new name[32];

	    if (sscanf(string, "s[230]", name))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] mensage] [nova mensagem] (Max caracteres: 230)");

	    format(BillBoardData[id][bbMessage], 32, name);

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou a mensagem do banner ID: %d para \"%s\".", ReturnName(playerid, 0), id, name);
	}
	else if (!strcmp(type, "dono", true))
	{
	    new giveplayerid;

	    if (sscanf(string, "d", giveplayerid))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] dono [playerid]");

        if (giveplayerid == INVALID_PLAYER_ID)
	    	return SendErrorMessage(playerid, "Este jogador está offline.");
		BillBoardData[id][bbOwner] = GetPlayerSQLID(giveplayerid);

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou o dono do banner ID: %d", ReturnName(playerid, 0), id);
	}
	else if (!strcmp(type, "removerdono", true))
	{
	    if (sscanf(string, "d"))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] [removeowner]");

		BillBoardData[id][bbOwner] = 0;

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s removeu o dono do banner ID: %d", ReturnName(playerid, 0), id);
	}
	else if (!strcmp(type, "range", true))
	{
	    new range;

	    if (sscanf(string, "d", range))
	        return SendSyntaxMessage(playerid, "/editarbanner [id] range [nova range]");

        if(range < 10)
		{
		    SendErrorMessage(playerid, "A range precisa estar entre 10-200");
		    return 1;
		}

		if(range > 200)
		{
		    SendErrorMessage(playerid, "A range precisa estar entre 10-200");
		    return 1;
		}

	    BillBoardData[id][bbRange] = range;

	    Billboard_Refresh(id);
	    Billboard_Save(id);

		SendAdminAlert(COLOR_LIGHTRED, "[ADMIN]: %s alterou a range do banner ID: %d para %d.", ReturnName(playerid, 0), id, range);
	}
	return 1;
}

