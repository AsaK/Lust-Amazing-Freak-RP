                format(Vehicles, sizeof(Vehicles), "");
            }
            format(strx, sizeof(strx), "  - %s (%s)\n", ReturnVehicleModelName(CarData[i][carModel]), CarData[i][carPlate]);
            strcat(Vehicles, strx);
		}
        format(stringx, sizeof(stringx), "{FFFFFF}Nome: {7AA3CC}%s\n{FFFFFF}Nascimento: {7AA3CC}%s\n{FFFFFF}Sexo: {7AA3CC}%s\n{FFFFFF}Origem: {7AA3CC}%s\n{FFFFFF}SSN: {7AA3CC}%d\n{FFFFFF}Celular: {7AA3CC}%d\n{FFFFFF}Endereço: {7AA3CC}%s\n{FFFFFF}Veículos:{7AA3CC}\n%s", ReturnName(targetid, 0), PlayerData[targetid][pBirthdate],
        (PlayerData[targetid][pGender] == 2) ? ("Feminino") : ("Masculino"), PlayerData[targetid][pOrigin], PlayerData[targetid][pCardID], PlayerData[targetid][pPhone], address, Vehicles);
        Dialog_Show(playerid, PersonStats, DIALOG_STYLE_MSGBOX, ReturnName(targetid, 0), stringx, "Confirmar", "Fechar");
	}
	else cmd_mdc(playerid, "\1");
	return 1;
}

