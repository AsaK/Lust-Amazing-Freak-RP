CMD:spawnpoint(playerid, params[])
{
	new point;
	if(sscanf(params, "i", point)) return SendErrorMessage(playerid, "/spawnpoint [0-2] (0 = Aeroporto, 1 = Facção, 2 = Última posição ao sair)");
	if(point == 0)
	{
	    SendClientMessage(playerid, COLOR_WHITE, "Você setou seu spawnpoint para o aeroporto.");
		PlayerData[playerid][pSpawnPoint] = 0;
		return 1;
	}
	if(point == 1)
	{
	    if(PlayerData[playerid][pFactionID] == -1)
	    {
	        SendErrorMessage(playerid, "Você não está em uma facção.");
	        return 1;
		}
		SendClientMessage(playerid, COLOR_WHITE, "Você setou seu spawnpoint para a facção.");
		PlayerData[playerid][pSpawnPoint] = 1;
		return 1;
	}
	if(point == 2)
	{
	    SendClientMessage(playerid, COLOR_WHITE, "Você setou seu spawnpoint para a última posição ao deslogar.");
		PlayerData[playerid][pSpawnPoint] = 2;
		return 1;
	}
	return 1;
}
CMD:fspawn(playerid, params[])
