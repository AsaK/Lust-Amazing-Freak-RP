		        SendServerMessage(playerid, "Você comprou o item garrafa de água de %s por $%d. Os itens foram adicionados ao inventário.", ReturnName(sellerid, 0), price);
		        SendServerMessage(sellerid, "%s comprou a água por $%d.", ReturnName(playerid, 0), price);
