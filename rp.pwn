CMD:descarregar(playerid, params[])
{
	new
		id = Business_NearestDeliver(playerid),
		vid = GetPlayerVehicleID(playerid);

	if (PlayerData[playerid][pJob] != JOB_COURIER)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

    if (id == -1)
	    return SendErrorMessage(playerid, "Você não está em nenhum ponto de entrega.");

 	if (!IsLoadableVehicle(vid))
  		return SendErrorMessage(playerid, "Você não está dentro de nenhum veículo carregável.");

	if (!CoreVehicles[vid][vehLoadType] || CoreVehicles[vid][vehLoads] < 1)
	    return SendErrorMessage(playerid, "Não há nenhuma caixa para entregas dentro do veículo.");

	if (PlayerData[playerid][pUnloading] != -1)
	    return SendErrorMessage(playerid, "Você já está descarregando um veículo.");

	if (PlayerData[playerid][pShipment] != -1 && id != PlayerData[playerid][pShipment])
	    return SendErrorMessage(playerid, "Você não aceitou entregas para essa empresa.");

	switch (CoreVehicles[vid][vehLoadType])
 	{
	 	case 1:
	 	{
	 	    if (BusinessData[id][bizType] != 1 && BusinessData[id][bizType] != 6)
	 	        return SendErrorMessage(playerid, "Essa empresa não pode aceitar este carregamento (tipo errado).");
		}
		case 2..4:
		{
		    if (BusinessData[id][bizType] != CoreVehicles[vid][vehLoadType])
	 	        return SendErrorMessage(playerid, "Essa empresa não pode aceitar este carregamento (tipo errado).");
		}
		case 5:
		{
		    if (BusinessData[id][bizType] != 6)
	 	        return SendErrorMessage(playerid, "Essa empresa não pode aceitar este carregamento (tipo errado).");
		}
		case 6:
		{
		    if (BusinessData[id][bizType] != 7)
	 	        return SendErrorMessage(playerid, "Essa empresa não pode aceitar este carregamento (tipo errado).");
		}
        case 7:
		{
		    if (BusinessData[id][bizType] != 8)
	 	        return SendErrorMessage(playerid, "Essa empresa não pode aceitar este carregamento (tipo errado).");
		}
	}
	static
	    Float:fX,
	    Float:fY,
	    Float:fZ;

	DisableWaypoint(playerid);

	GetVehicleBoot(vid, fX, fY, fZ);
	RemovePlayerFromVehicle(playerid);

	PlayerData[playerid][pUnloading] = id;
	PlayerData[playerid][pUnloadVehicle] = vid;

	SetPlayerCheckpoint(playerid, fX, fY, fZ, 1.0);
	SendServerMessage(playerid, "Descarregue e entregue as caixas na porta da empresa.");

	return 1;
}

CMD:reparar(playerid, params[])
{
	if (PlayerData[playerid][pJob] != JOB_MECHANIC)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (IsPlayerInAnyVehicle(playerid))
	    return SendErrorMessage(playerid, "Você precisa sair do veículo.");

	if (!Inventory_HasItem(playerid, "Kit de reparos"))
	    return SendErrorMessage(playerid, "Você não tem um kit de reparos.");

	if (PlayerData[playerid][pRepairTime] > gettime())
	    return SendErrorMessage(playerid, "Você precisa esperar %d segundos para reparar novamente.", PlayerData[playerid][pRepairTime] - gettime());

	for (new i = 1; i != MAX_VEHICLES; i ++) if (IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
	    if (!IsEngineVehicle(i))
	        return SendErrorMessage(playerid, "Esse veículo não pode ser reparado.");

	    if (!GetHoodStatus(i))
	        return SendErrorMessage(playerid, "Você precisa abrir o capô antes de reparar o veículo.");

        if (CoreVehicles[i][vehRepairing])
            return SendErrorMessage(playerid, "Esse veículo já está sendo reparado.");

		Inventory_Remove(playerid, "Kit de reparos");
		ApplyAnimation(playerid, "BD_FIRE", "wash_up", 4.1, 0, 0, 0, 0, 0, 1);

        CoreVehicles[i][vehRepairing] = true;
        SetTimerEx("RepairCar", 5000, false, "dd", playerid, i);

        SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s começa a reparar o veículo.", ReturnName(playerid, 0));
		GameTextForPlayer(playerid, "~n~~n~~n~~n~~n~~n~~n~~n~~n~~n~~g~Reparando...~w~ Aguarde", 5500, 3);
		return 1;
	}
	SendErrorMessage(playerid, "Você não está próximo de nenhum capô de um veículo.");
	return 1;
}

CMD:adicionarnitro(playerid, params[])
{
	if (PlayerData[playerid][pJob] != JOB_MECHANIC)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (IsPlayerInAnyVehicle(playerid))
	    return SendErrorMessage(playerid, "Você precisa sair do veículo.");

	if (!Inventory_HasItem(playerid, "NOS"))
	    return SendErrorMessage(playerid, "Você não tem um cilindro de NOS no inventário.");

	for (new i = 1; i != MAX_VEHICLES; i ++) if (IsValidVehicle(i) && IsPlayerNearHood(playerid, i))
	{
	    if (!IsEngineVehicle(i) || IsABike(i) || IsABoat(i) || IsAPlane(i) || IsAHelicopter(i))
	        return SendErrorMessage(playerid, "Você não pode colocar NOS neste veículo.");

	    if (!GetHoodStatus(i))
	        return SendErrorMessage(playerid, "O capô precisa estar aberto.");

		Inventory_Remove(playerid, "NOS");
		ApplyAnimation(playerid, "BD_FIRE", "wash_up", 4.1, 0, 0, 0, 0, 0, 1);

		AddComponent(i, 1010);
		SendNearbyMessage(playerid, 30.0, COLOR_PURPLE, "** %s coloca um cilindro de NOS conectado ao motor do veículo.", ReturnName(playerid, 0));
		return 1;
	}
	SendErrorMessage(playerid, "Você não está próximo de nenhum capô de um veículo.");
	return 1;
}

