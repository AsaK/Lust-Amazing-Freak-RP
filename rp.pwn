CMD:pegarlixo(playerid, params[])
{
	new
		id = Garbage_Nearest(playerid),
		string[64];

	if (PlayerData[playerid][pJob] != JOB_GARBAGE)
	    return SendErrorMessage(playerid, "Você não está no emprego correto.");

	if (id == -1)
	    return SendErrorMessage(playerid, "Você não está próximo de uma lata de lixo.");

	if (GarbageData[id][garbageCapacity] < 1)
