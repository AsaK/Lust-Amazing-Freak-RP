Dialog:TrackNumber(playerid, response, listitem, inputtext[])
{
	if (GetFactionType(playerid) != FACTION_POLICE || !IsACruiser(GetPlayerVehicleID(playerid)))
	    return 0;

	if (response)
	{
        if(!IsNumeric(inputtext)) return Dialog_Show(playerid, TrackNumber, DIALOG_STYLE_INPUT, "Rastrear Celular", "Insira apenas números!", "Confirmar", "Voltar");
        new num = strval(inputtext);
        foreach(new i : Player){
            if(PlayerData[i][pPhone] == num && Inventory_HasItem(i, "Celular")){
                if(PlayerData[i][pPhoneOff]) return SendErrorMessage(playerid, "Este celular está desligado!");
                new Float:x,Float:y,Float:z;
                GetPlayerPos(i, x, y, z);
                SetPlayerCheckpoint(playerid, x, y, z, 2.0);
                SendServerMessage(playerid, "Um checkpoint foi setado no local do rastreamento.");
                return 1;
            } else {
                return SendErrorMessage(playerid, "Este celular não existe!");
            }
        }
	}
	else cmd_mdc(playerid, "\1");
	return 1;
}

