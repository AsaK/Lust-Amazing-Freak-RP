CMD:vender(playerid, params[])
{
	static
	    targetid,
	    type[24],
	    string[128];

	if (sscanf(params, "us[24]S()[128]", targetid, type, string))
	{
	    SendSyntaxMessage(playerid, "/vender [playerid/nome] [opção]");
	    SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} casa, empresa, veiculo");
	    return 1;
	}
	if (targetid == INVALID_PLAYER_ID || !IsPlayerNearPlayer(playerid, targetid, 5.0))
	{
		SendErrorMessage(playerid, "O jogador está desconectado ou longe de você.");
		return 1;
	}
	if (targetid == playerid)
	{
		SendErrorMessage(playerid, "Você não pode vender para si.");
		return 1;
	}
	if (!strcmp(type, "casa", true))
	{
		static
		    price,
			houseid = -1;

		if (sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/vender [playerid/nome] casa [valor]");

		if (price < 1)
		    return SendErrorMessage(playerid, "O valor precisa ser maior que 1.");

		if ((houseid = House_Nearest(playerid)) != -1 && House_IsOwner(playerid, houseid)) {
			PlayerData[targetid][pHouseSeller] = playerid;
			PlayerData[targetid][pHouseOffered] = houseid;
			PlayerData[targetid][pHouseValue] = price;

		    SendServerMessage(playerid, "Você ofereceu sua casa para %s por %s.", ReturnName(targetid, 0), FormatNumber(price));
            SendServerMessage(targetid, "%s ofereceu uma casa por %s (Use \"/aceitar casa\" para aceitar).", ReturnName(playerid, 0), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Você precisa estar próximo da sua casa.");
	}
	else if (!strcmp(type, "empresa", true))
	{
		static
		    price,
			bizid = -1;

		if (sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/vender [playerid/nome] empresa [valor]");

		if (price < 1)
		    return SendErrorMessage(playerid, "O valor precisa ser maior que 1.");

		if ((bizid = Business_Nearest(playerid)) != -1 && Business_IsOwner(playerid, bizid)) {
			PlayerData[targetid][pBusinessSeller] = playerid;
			PlayerData[targetid][pBusinessOffered] = bizid;
			PlayerData[targetid][pBusinessValue] = price;

		    SendServerMessage(playerid, "Você ofereceu sua empresa para %s por %s.", ReturnName(targetid, 0), FormatNumber(price));
            SendServerMessage(targetid, "%s ofereceu uma empresa por %s (Use \"/aceitar casa\" para aceitar).", ReturnName(playerid, 0), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Você precisa estar próximo da sua empresa.");
	}
	else if (!strcmp(type, "veiculo", true))
	{
		static
		    price,
			carid = -1;

		if (sscanf(string, "d", price))
			return SendSyntaxMessage(playerid, "/vender [playerid/nome] veiculo [valor]");

		if (price < 1)
		    return SendErrorMessage(playerid, "O valor precisa ser maior que 1.");

		if ((carid = Car_Inside(playerid)) != -1 && Car_IsOwner(playerid, carid)) {
            if(CarData[carid][carModel] == 545 || CarData[carid][carModel] == 575 || CarData[carid][carModel] == 580 || CarData[carid][carModel] == 593)
                return SendErrorMessage(playerid, "Você não pode vender um veículo de Donater!");
			PlayerData[targetid][pCarSeller] = playerid;
			PlayerData[targetid][pCarOffered] = carid;
			PlayerData[targetid][pCarValue] = price;

		    SendServerMessage(playerid, "Você ofereceu seu carro para %s por %s.", ReturnName(targetid, 0), ReturnVehicleModelName(CarData[carid][carModel]), FormatNumber(price));
            SendServerMessage(targetid, "%s ofereceu o %s por %s (Use \"/aceitar veiculo\" para aceitar).", ReturnName(playerid, 0), ReturnVehicleModelName(CarData[carid][carModel]), FormatNumber(price));
		}
		else SendErrorMessage(playerid, "Você não está dentro de nenhum veículo.");
	}
	return 1;
}

CMD:aceitar(playerid, params[])
{
	if (isnull(params))
 	{
	 	SendSyntaxMessage(playerid, "/aceitar [opção]");
		SendClientMessage(playerid, COLOR_YELLOW, "[OPÇÕES]:{FFFFFF} casa, empresa, veiculo, comida, faccao, cumprimento, revistar");
		return 1;
	}
	if (!strcmp(params, "casa", true) && PlayerData[playerid][pHouseSeller] != INVALID_PLAYER_ID)
	{
	    new
	        sellerid = PlayerData[playerid][pHouseSeller],
	        houseid = PlayerData[playerid][pHouseOffered],
	        price = PlayerData[playerid][pHouseValue];

		if (!IsPlayerNearPlayer(playerid, sellerid, 6.0))
		    return SendErrorMessage(playerid, "Você não está próximo deste jogador.");

		if (GetMoney(playerid) < price)
		    return SendErrorMessage(playerid, "Você não tem dinheiro para comprar a casa.");

		if (House_Nearest(playerid) != houseid)
		    return SendErrorMessage(playerid, "Você precisa estar perto para comprar isso.");

		if (!House_IsOwner(sellerid, houseid))
		    return SendErrorMessage(playerid, "A oferta para compra da casa foi cancelada.");

		SendServerMessage(playerid, "Você comprou a casa de %s por %s.", ReturnName(sellerid, 0), FormatNumber(price));
		SendServerMessage(sellerid, "%s comprou sua casa por %s.", ReturnName(playerid, 0), FormatNumber(price));

		HouseData[houseid][houseOwner] = GetPlayerSQLID(playerid);
		House_Save(houseid);

		GiveMoney(playerid, -price);
		GiveMoney(sellerid, price);

		Log_Write("logs/offer_log.txt", "[%s] %s (%s) vendeu a casa para %s (%s) por %s.", ReturnDate(), ReturnName(playerid, 0), PlayerData[playerid][pIP], ReturnName(sellerid, 0), PlayerData[sellerid][pIP], FormatNumber(price));

		PlayerData[playerid][pHouseSeller] = INVALID_PLAYER_ID;
		PlayerData[playerid][pHouseOffered] = -1;
		PlayerData[playerid][pHouseValue] = 0;
	}
	if (!strcmp(params, "empresa", true) && PlayerData[playerid][pBusinessSeller] != INVALID_PLAYER_ID)
	{
	    new
	        sellerid = PlayerData[playerid][pBusinessSeller],
	        bizid = PlayerData[playerid][pBusinessOffered],
	        price = PlayerData[playerid][pBusinessValue];

		if (!IsPlayerNearPlayer(playerid, sellerid, 6.0))
		    return SendErrorMessage(playerid, "Você não está próximo deste jogador.");

		if (GetMoney(playerid) < price)
		    return SendErrorMessage(playerid, "Você não tem dinheiro para comprar a empresa.");

		if (Business_Nearest(playerid) != bizid)
		    return SendErrorMessage(playerid, "Você precisa estar perto da empresa para compra-la.");

		if (!Business_IsOwner(sellerid, bizid))
		    return SendErrorMessage(playerid, "A oferta para compra da empresa foi cancelada.");

		SendServerMessage(playerid, "Você comprou a empresa de %s por %s.", ReturnName(sellerid, 0), FormatNumber(price));
		SendServerMessage(sellerid, "%s comprou sua empresa por %s.", ReturnName(playerid, 0), FormatNumber(price));

		BusinessData[bizid][bizOwner] = GetPlayerSQLID(playerid);
		Business_Save(bizid);

		GiveMoney(playerid, -price);
		GiveMoney(playerid, price);

		Log_Write("logs/offer_log.txt", "[%s] %s (%s) comprou a empresa %s (%s) por %s.", ReturnDate(), ReturnName(playerid, 0), PlayerData[playerid][pIP], ReturnName(sellerid, 0), PlayerData[sellerid][pIP], FormatNumber(price));

		PlayerData[playerid][pBusinessSeller] = INVALID_PLAYER_ID;
		PlayerData[playerid][pBusinessOffered] = -1;
		PlayerData[playerid][pBusinessValue] = 0;
	}
	if (!strcmp(params, "veiculo", true) && PlayerData[playerid][pCarSeller] != INVALID_PLAYER_ID)
	{
	    new
	        sellerid = PlayerData[playerid][pCarSeller],
	        carid = PlayerData[playerid][pCarOffered],
	        price = PlayerData[playerid][pCarValue];

		if (!IsPlayerNearPlayer(playerid, sellerid, 6.0))
		    return SendErrorMessage(playerid, "Você não está próximo deste jogador.");

		if (GetMoney(playerid) < price)
		    return SendErrorMessage(playerid, "Você não tem dinheiro para comprar vehicle.");

		if (Car_Nearest(playerid) != carid)
		    return SendErrorMessage(playerid, "Você precisa estar perto the vehicle to purchase it.");

		if (!Car_IsOwner(sellerid, carid))
		    return SendErrorMessage(playerid, "A oferta para comprar este veiculo foi cancelada.");

		SendServerMessage(playerid, "Você comprou de %s o veículo %s por %s.", ReturnName(sellerid, 0), ReturnVehicleModelName(CarData[carid][carModel]), FormatNumber(price));
		SendServerMessage(sellerid, "%s comprou seu %s por %s.", ReturnName(playerid, 0), ReturnVehicleModelName(CarData[carid][carModel]), FormatNumber(price));

		CarData[carid][carOwner] = GetPlayerSQLID(playerid);
		Car_Save(carid);

		GiveMoney(playerid, -price);
		GiveMoney(sellerid, price);

		Log_Write("logs/offer_log.txt", "[%s] %s (%s) vendeu o %s para %s (%s) por %s.", ReturnDate(), ReturnName(playerid, 0), PlayerData[playerid][pIP], ReturnVehicleModelName(CarData[carid][carModel]), ReturnName(sellerid, 0), PlayerData[sellerid][pIP], FormatNumber(price));

		PlayerData[playerid][pCarSeller] = INVALID_PLAYER_ID;
		PlayerData[playerid][pCarOffered] = -1;
		PlayerData[playerid][pCarValue] = 0;
