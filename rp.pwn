	        return SendErrorMessage(playerid, "O jogador está desconectado.");

		if (!PlayerData[targetid][pTaxiCalled])
		    return SendErrorMessage(playerid, "O pedido de taxi desse jogador já foi aceito.");

		static
			Float:x,
			Float:y,
			Float:z;

